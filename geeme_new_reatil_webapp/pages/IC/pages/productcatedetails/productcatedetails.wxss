/* pages/IC/pages/productcatedetails/productcatedetails.wxss */
.container {
  background: #f7f7f7;
  min-height: 100vh;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* 服务主体 */

.body {
  width: 100%;
  height: auto;
}

/* 服务头部 */

.b-head {
  width: 100%;
  height: 100rpx;
  color: #444;
  font-size: 28rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  background: white;
  position: relative;
}

.b-h-left {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
}

.b-l-text {
  margin-right: 8rpx;
}

.b-l-icon {
  width: 18rpx;
  height: 18rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.b-h-middle {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
}

.b-m-text {
  margin-right: 8rpx;
}

.b-m-icon {
  width: 18rpx;
  height: 18rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.b-h-right {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
}

.b-r-text {
  margin-right: 8rpx;
}

.b-r-icon {
  width: 18rpx;
  height: 18rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

/* 服务主体 */

.b-content {
  width: 100%;
  height: auto;
  padding: 32rpx 32rpx 150rpx;
}

/* 缺省 */
.default {
  width: 100%;
  height: auto;
  padding-top: 254rpx;
}

.default-icon {
  width: 252rpx;
  height: 178rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  margin: 0 auto 28rpx;
}

.default-text {
  width: 100%;
  line-height: 40rpx;
  color: #888;
  font-size: 28rpx;
  text-align: center;
}


.bg{
  width: 100%;
  height: calc( 100vh - 203rpx );
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 100rpx;
  right: 0;
  z-index: 500;
  display: none;
}
.bg.active{
  display: block;
}