@import '/utils/wxss/icon.wxss';
page {
  position: relative;
  padding-bottom: 138rpx;
  min-height: 100vh;
}

.header {
  position: relative;
  height: 260rpx;
  overflow:hidden;
}

.header-bg {
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  width: 900rpx;
  margin-left: -75rpx;
  height: 225rpx;
  background: #f52d56;
  border-radius: 90%/0 0 100% 100%;
}
.car-count{
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  border: 4rpx solid #FFFFFF;
  background: #F52D56;
  top: -15rpx;
  right: -15rpx;
  color: #FFFFFF;
  font-size: 20rpx;
  line-height: 24rpx;
  text-align: center;
}

.selector{
  width: 100%;
  left: 0;
  top: -2rpx;
  position: fixed;
  z-index: 2;
  /* height: 70rpx; */
  box-sizing: border-box;
  padding: 0 30rpx;
}
.shade{
  position: fixed;
  top: 102rpx;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.5);
  overflow-y:hidden;
  z-index: 4;
}
.options{
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: #FFFFFF;
  box-sizing: border-box;
  padding-left: 30rpx;
  z-index: 4;
}
.options-item{
  border-bottom: 2rpx solid #eeeeee;
  padding: 28rpx 0;
  z-index: 4;
}
.store-thumb{
  display: block;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background: #999999;
  margin-right: 12rpx;
}

.search-container {
  height: 64rpx;
  width: 664rpx;
}

/* shop */

.shopname {
  width: 100%;
  height: 195rpx;
  padding-left: 60rpx;
  padding-top: 124rpx;
  display: flex;
  flex-wrap: nowrap;
  position: relative;
}

.shop-left {
  width: 130rpx;
  height: 130rpx;
  margin-right: 28rpx;
  border-radius: 50%;
  box-shadow: 0rpx 4rpx 20rpx 0rpx rgba(0, 0, 0, 0.2);
  margin-top: -19rpx;
}

.shop-right {
  width: calc(100vw-130rpx);
  height: 130rpx;
  padding-top: 3rpx;
  color: white;
}

.shop-info {
  width: 400rpx;
  height: 44rpx;
  font-size: 32rpx;
}

.shop-phone {
  height: 34rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  overflow: hidden;
  display: box;
  text-overflow: ellipsis;
  box-orient: vertical;
  line-clamp: 1;
}

/* 功能 */

.func {
  width: 100%;
  padding: 0 60rpx;
  display: flex;
  flex-wrap: wrap;
  /* justify-content: space-between; */
  margin: 26rpx 0;
}


.func-item {
  width: 116rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  margin-right: 55rpx;
  margin-bottom: 34rpx;
}

.func-item:nth-child(4){
  margin-right: 0;
}

.func-image {
  width: 116rpx;
  height: 116rpx;
  margin-bottom: 20rpx;
}

.func-text {
  width: 116rpx;
  height: 34rpx;
  text-align: center;
  line-height: 34rpx;
  font-size: 24rpx;
  color: rgba(38, 50, 56, 1);
}

/* 功能 */

.funclass {
  width: 100%;
  padding: 0 38rpx;
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 26rpx;
  margin-right: 30rpx;
}

.funclass-item {
  width: 130rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  margin-bottom: 34rpx;
  margin-right: 5rpx;
}

.funclass-item:nth-child(5n){
  margin-right: 0;
}

.funclass-image {
  display: block;
  width: 88rpx;
  height: 88rpx;
  margin-bottom: 20rpx;
}

.funclass-text {
  width: 130rpx;
  height: 34rpx;
  text-align: center;
  line-height: 34rpx;
  font-size: 24rpx;
  color: rgba(38, 50, 56, 1);
}


.swiper-adv{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.adv-item{
  width: 684rpx;
  height: 256rpx;
}

/* 推荐美店 */

.recommended {
  width: 100%;
  margin-bottom: 40rpx;
}

.recommended-top {
  width: 100%;
  height: 56rpx;
  font-size: 37rpx;
  font-weight: 600;
  font-family: PingFangSC-Semibold;
  color: #262628;
  line-height: 56rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  padding: 0 32rpx;
  margin-bottom: 26rpx;
}

.recommended-icon {
  width: 30rpx;
  height: 100%;
  background: no-repeat center center/100% auto;
  background-size: 14rpx;
}

.recommended-worksBottom {
  padding: 0 32rpx;
}

.recommend-scroll-view{
  width:100%;
  white-space: nowrap;
}

.recommend-item{
  display: inline-block;
  margin-left:30rpx;
  padding-bottom:14rpx;
  width:270rpx;
  border-radius:8rpx;
  box-shadow: 0 0 8rpx 0 rgba(0, 0, 0, .1);
  overflow: hidden;
}

.recommend-item:last-child{
  margin-right:30rpx;
}

.recommend-image{
  height:270rpx;
}

.recommend-image > image{
  width:100%;
  height:100%;
}

.recommend-text{
  padding:0 10rpx;
}

.recommend-title{
  margin-top:10rpx;
  height:70rpx;
  color:#333;
  font-size:26rpx;
  white-space: normal;
}

.price-group{
  margin-top:10rpx;
  font-size:30rpx;
  color:#f52d56;
  font-weight: 600;
}

.price-shopPrice{
  margin-top:8rpx;
  color:#333;
  font-size:24rpx;
}

.word-symbol{
  display: inline-block;
  box-sizing: border-box;
  vertical-align: middle;
  width:24rpx;
  height:24rpx;
  line-height:24rpx;
  font-size:16rpx;
  text-align: center;
  border-radius:4rpx;
  font-weight: normal;
}

.price-group > .word-symbol{
  background: linear-gradient(to right, #ff3a31, #f138c9);
  color:#fff;
  vertical-align: 4rpx;
}

.price-shopPrice > .word-symbol{
  border: 1rpx solid #666;
  vertical-align: 2rpx;
}

.buttons {
  width: 100%;
  height: 120rpx;
  position: fixed;
  bottom: 0;
  right: 0;
  background-color: white;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-end;
  align-items: center;
  border-top: 2rpx solid #efeff4;
  z-index: 999;
  font-size: 26rpx;
}

.cover {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
  background: rgba(0, 0, 0, 0.5);
}

.redPacket-container {
  position: fixed;
  left: 25rpx;
  top: 30rpx;
  width: 700rpx;
  height: 1000rpx;
  text-align: center;
  z-index: 11;
  /* background:url('/pages/tabBar/home/index/bg_redPacket1.png'); */
  background-size: 100% 100%;
}

.redPacket-container-text{
  margin-top:200rpx;
  color:#fff;
  font-size:24rpx;
}

.redPacket {
  display: inline-block;
  width: 510rpx;
  background-size: 100% 100%;
}

.redPacket-button {
  margin: 0 auto;
  width: 420rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 40rpx;
  background-color: #fff;
  color: #e1002e;
  font-size: 32rpx;
}

.redPacket-button.button2{
  position: absolute;
  left: 50%;
  bottom:180rpx;
  transform: translateX(-50%);
}

.redPacket-info {
  position: relative;
  display: inline-block;
  padding: 0 40rpx;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: left;
}

.redPacket-info-left {
  display: inline-block;
  line-height: normal;
  color: #fff;
  vertical-align: middle;
}

.redPacket-info-left > view:first-child {
  font-size: 36rpx;
}

.redPacket-info-left > view:nth-child(2) {
  margin-top: 6rpx;
  font-size: 24rpx;
}

.redPacket-info-right {
  position: absolute;
  right: 10rpx;
  top: 0;
  width: 160rpx;
  height: 100%;
  color: #e1002e;
  font-size: 54rpx;
  font-weight: 600;
  text-align: center;
}

.redPacket-info-right > text {
  font-weight: normal;
  font-size: 24rpx;
}

.redPacket-info2{
  position: absolute;
  width:100%;
  top:250rpx;
  text-align: center;
  color: #e1272f;
}

.redPacket-info2 > view:nth-child(1){
  font-weight: bold;
  font-size:48rpx;
}

.redPacket-info2 > view:nth-child(2){
  font-size:28rpx;
  margin-top:20rpx;
}

.redPacket-info2 > view:nth-child(3){
  margin-top:50rpx;
  font-size:120rpx;
}

.redPacket.big {
  margin-top: 140rpx;
  margin-bottom: 100rpx;
  height: 336rpx;
  line-height: 336rpx;
  background-image: url('data:image/png;base64,');
}

.redPacket.middle {
  margin-bottom:40rpx;
  height: 196rpx;
  line-height: 196rpx;
  background-image: url('data:image/png;base64,');
}

.redPacket.middle:first-child{
  margin-top: 130rpx;
}

.redPacket.middle:nth-child(2){
  margin-bottom:68rpx;
}

.redPacket.small{
  margin-bottom:32rpx;
  height: 140rpx;
  line-height: 140rpx;
  background-image: url('data:image/png;base64,');
}

.redPacket.small:first-child{
  margin-top: 130rpx;
}

.redPacket.small:nth-child(3){
  margin-bottom:56rpx;
}

.redPacket-container > .icon--close_big{
  position: absolute;
  right:10rpx;
  top:24rpx;
  width:60rpx;
  height:60rpx;
  background-size:100% 100%;
}

.redPacket-close-btn{
  position: absolute;
  bottom:0;
  left:50%;
  transform: translateX(-50%);
  width: 80rpx;
  height:80rpx;
}

.Service{
  width: 100%;
  height: auto;
  padding: 0 32rpx;
}
.Service-item{
  width: 100%;
  height: auto;
}

.s-title{
  height: 104rpx;
  line-height:104rpx;
  display: flex;
}

.s-title-left{
  color: #262628 !important;
  font-size: 40rpx !important;
  font-weight: 600 !important;
}

.s-title-right{
  float:right;
  width:18rpx;
  height:100%;
  background-size: 18rpx 28rpx;
  background-position: center center;
  background-repeat: no-repeat;
}

.s-contents{
  width: 100%;
  height: auto;
}


/* 充值有礼 */
.purchaseRecharge{
  padding-bottom:150rpx;
}

.card-kind{
  margin-top:60rpx;
}

.card-kind.bigCard{
  margin-top:0;
}

.kind-title {
  width: 100%;
  height: 80rpx;
  line-height:80rpx;
  padding: 0 32rpx;
  color: #262628;
  font-size: 40rpx;
  font-weight: 600;
  overflow:hidden;
}

.bigCard .kind-title{
  height:100rpx;
  line-height:100rpx;
}

.kind-title > view{
  display: inline-block;
}

.title-right{
  float:right;
  width:16rpx;
  height:100%;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size:100% 30rpx;
}

.card-container {
  padding-left:32rpx;
  white-space: nowrap;
}

.card {
  position:relative;
  display: inline-block;
  width: 362rpx;
  height: 230rpx;
  border-radius: 15rpx;
  margin-right: 20rpx;
  background-size: 100% 100%;
  color:white;
}

.bigCard .card{
  width:460rpx;
  height:292rpx;
  margin-right:30rpx;
}

.card-title{
  padding-left:20rpx;
  height:76rpx;
  line-height:76rpx;
  font-size:0;
}

.bigCard .card-title{
  height:96rpx;
  line-height:96rpx;
}

.card-logo {
  margin-right:8rpx;
  width: 46rpx;
  height: 46rpx;
  border-radius:50%;
  vertical-align: middle;
}

.bigCard .card-logo{
  margin-right:10rpx;
  width:56rpx;
  height:56rpx;
}

.card-name {
  display: inline-block;
  font-size:28rpx;
  vertical-align: middle;
}

.bigCard .card-name{
  font-size:36rpx;
}

.card-info{
  position:absolute;
  bottom:6rpx;
  width:100%;
  text-align: right;
  padding:0 14rpx;
}

.card-effect > text{
  padding:0 12rpx;
  height:26rpx;
  line-height:26rpx;
  font-size:20rpx;
  color:#333;
  border-radius:13rpx;
  background-color:#fff;
}

.card-price{
  font-size:32rpx;
}

.bigCard .card-price{
  font-size:40rpx;
}