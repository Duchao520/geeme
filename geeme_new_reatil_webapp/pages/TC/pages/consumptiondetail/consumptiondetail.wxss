page {
  background-color: #f7f7f7;
}

.consumptiondetail {
  width: 100%;
  padding: 46rpx 32rpx 200rpx;
}

.topLine {
  width: 710rpx;
  height: 24rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  position: absolute;
  top: 46rpx;
  left: 0;
  right: 0;
  margin: auto;
  z-index: 999;
}

.container {
  width: 686rpx;
  height: auto;
}

/* 头部 */

.header {
  width: 100%;
  height: auto;
  background-color: white;
  padding: 28rpx 32rpx 0;
  position: relative;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}

.header-left {
  height:80rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}

.storeImg {
  width: 76rpx;
  height: 76rpx;
  margin-right: 12rpx;
  border-radius:8rpx;
}

.storeName {
  color: #333;
  font-size: 28rpx;
}

.consumptionTime {
  margin-top:10rpx;
  color: #999;
  font-size: 24rpx;
}

.status {
  width: 92rpx;
  height: 38rpx;
  color: white;
  font-size: 24rpx;
  text-align: center;
  line-height: 38rpx;
  border-radius: 4rpx;
}

.status.UnBilling{
  background-color: #f52d56;
}
.status.UnPaid {
  background-color: #F5A623;
}
.status.Paid{
  background-color: #00CF82;
}

/* 凹槽 */

.bump {
  width: 100%;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

/* 服务信息 */

.serviceContent {
  width: 100%;
  height: auto;
  padding: 0 32rpx;
  background-color: white;
}

.serviceItem {
  width: 100%;
  height: auto;
  margin-top: 24rpx;
}

.serviceItem:first-child {
  margin-top: 0;
}

.serviceOne {
  width: 100%;
  height: 120rpx;
  position: relative;
  margin-bottom: 26rpx;
}

.serviceImg {
  float: left;
  width: 120rpx;
  height: 120rpx;
  margin-right: 14rpx;
  background-color: black;
}

.serviceName {
  height: 40rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 8rpx;
}

.serviceNameLeft {
  color: #333;
  font-size: 28rpx;
  height:34rpx;
  line-height:34rpx;
}

.serviceNameLeft > text{
  margin-left:10rpx;
  display: inline-block;
  vertical-align: top;
  width:76rpx;
  height:34rpx;
  line-height:34rpx;
  font-size:24rpx;
  text-align: center;
  background-color:rgba(245, 45, 86, .1);
  color:#f52d56;
  border:1px solid #f52d56;
}

.serviceNameLeft > text.blue{
  color:#58c9f3;
  background-color: rgba(88, 201, 243, .1);
  border-color:#58c9f3;
}

.serviceNameLeft > text.green{
  color: #75c74f;
  background-color: rgba(184, 233, 134, .2);
  border-color: #75c74f;
}

.serviceNameright {
  color: #333;
  font-size: 24rpx;
}

.servicePackage {
  color: #999;
  font-size: 24rpx;
  margin-bottom: 6rpx;
}

.staffPeople {
  margin-right:70rpx;
  color: #999;
  font-size: 24rpx;
}

.service-line text {
  margin-right: 16rpx;
}

.upgrade{
  display: inline-block;
  height: 36rpx;
  padding-left: 45rpx;
  background:url('data:image/png;base64,') no-repeat left center;
  background-size: 36rpx 36rpx;
}

.serviceCount {
  position: absolute;
  bottom: 0;
  right: 0;
  color: #999;
  font-size: 24rpx;
}

.serviceTwo {
  color:#999;
  font-size:24rpx;
  margin-bottom: 20rpx;
}

.discountBox {
  height: auto;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  margin-top: 16rpx;
}

.discountBox:first-child {
  margin-top: 0;
}

.serviceThree {
  width: 100%;
  height: auto;
  padding-bottom: 24rpx;
  border-bottom: 2rpx dashed rgba(239, 239, 244, 1);
}

.payPrice {
  color: #333;
  font-size: 24rpx;
  text-align: right;
  margin-bottom: 16rpx;
}

.orderType {
  color: #333;
  font-size: 24rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-end;
}

.line {
  color: #979797;
  margin: 0 20rpx;
}

/* 代付金额 */

.waitPay {
  width: 100%;
  padding-top: 16rpx;
  min-height: 224rpx;
  color: #333;
  font-size: 24rpx;
  border-bottom: 2rpx dashed rgba(239, 239, 244, 1);
}

/* 订单信息 */

.orderInfo {
  width: 100%;
  padding: 16rpx 0 38rpx;
}

.orderSerial {
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  margin-bottom: 16rpx;
}

.orderSerial:last-child {
  margin-bottom: 0;
}

.orderText {
  color: #999;
  font-size: 24rpx;
}

.orderNumber {
  color: #333;
  font-size: 24rpx;
}

/* 底部按钮 */

.button {
  width: 100%;
  padding: 30rpx 0;
  position: fixed;
  bottom: 0;
  right: 0;
  background-color: #fff;
}

.calculate {
  width: 684rpx;
  height: 100rpx;
  border-radius: 50rpx;
  background-color: #f52d56;
  color: white;
  font-size: 34rpx;
  text-align: center;
  line-height: 100rpx;
  margin: 0 auto;
}

.evaluation {
  width: 684rpx;
  height: 100rpx;
  border-radius: 50rpx;
  background-color: #f52d56;
  color: white;
  font-size: 34rpx;
  text-align: center;
  line-height: 100rpx;
  margin: 0 auto;
}
