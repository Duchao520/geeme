.UsersMyAssets{
  width: 100%;
  height: auto;
}
.userAssetsInfo{
  
}
.userAssetsInfoTitle{
  color: #FFFFFF;
  font-size: 36rpx;
}
.userAssetsInfoNumber{
  height: 84rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
} 
.userAssetsInfoNumber-l{
  color: #F7F1BE;
  font-size: 60rpx;
  margin-right: 8rpx;
}
.userAssetsInfoNumber-r{
  padding-top: 28rpx;
}
.userAssetsInfoNumber-r-item{
  width: 130rpx;
  border-radius: 15rpx;
  border: 1rpx solid white;
  font-size: 24rpx;
  color: white;
  text-align: center;
  line-height: 34rpx;
}



.vipCardImage{
  width: 700rpx;
  height: 323rpx;
  border-radius: 15rpx;
  margin: 32rpx  auto 44rpx; 
  padding: 32rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  overflow: hidden;
}

.btn{
  width: 92rpx;
  height: 46rpx;
  border-radius: 2px;
  border: 1px solid rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 46rpx;
  margin-right: 20rpx;
}
.hover-btn{
  background: #FFFFFF;
  color: #000000!important;
}
.btn:last-child{
  margin-right: 0;
}

/* 资产信息 */
.assetsContainer{
  width: 686rpx;
  box-sizing: border-box;
  padding: 40rpx 22rpx;
  border-radius: 10rpx;
  box-shadow: 0 0 16rpx 0 rgba(174, 184, 229, .3);
  margin: 0 auto;
}
.assetsInfo{
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  margin: 0 auto;
  padding: 28rpx 0;
  border-bottom: 2rpx solid #EBEBEB;
}
.assetsInfo:first-child{
  padding-top: 0;
}
.assetsInfo:last-child{
  border: none;
  padding-bottom: 0;
}
.assetsInfo-l{
  width: 50%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
  padding-left: 40rpx;
  border-right: 2rpx solid #EBEBEB;
}
.icon{
  width: 60rpx;
  height: 60rpx;
  margin-right: 12rpx;
}
.vipCardIcon{
  width: 60rpx;
  height: 60rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  border-radius: 50%;
  margin-right: 12rpx;
}
.redEnvelopeCardIcon{
    width: 60rpx;
  height: 60rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  border-radius: 50%;
  margin-right: 12rpx;
}
.typeInfo{

}
.typeCount{
  color: #333333;
  font-size: 32rpx;
}
.typeName{
  color: #999999;
  font-size: 24rpx; 
}

.assetsInfo-r{
  width: 50%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
  padding-left: 40rpx;
}

.switch{
  justify-content: center;
}
.switch switch{
  zoom: .5;
}


/* 权益金 */
.equity {
  width: 686rpx;
  height: 180rpx;
  margin: 0 auto 32rpx;
  border-radius: 8rpx;
  box-shadow: 0rpx 0rpx 16rpx 0rpx rgba(174, 184, 229, 0.3);
}
.equity-image {
  width: 120rpx;
  height: 120rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}
.equity-image {
  margin-left: 42rpx;
}
.equity-amount ,.equity-amount view {
  width: 392rpx;
}
.equity-amount {
  margin-left: 20rpx;
}
.equity-amount view:nth-child(1) text {
  color: #333;
  font-size: 48rpx;
}
.equity-amount view:nth-child(1) text:nth-child(1) {
  font-size: 32rpx;
}
.equity-amount view:nth-child(2) {
  margin-top: 4rpx;
}
.equity-amount view:nth-child(2) text {
  color: #999;
  font-size: 24rpx;
}
.equity-amount view:nth-child(2) text:nth-child(2) {
  width: 106rpx;
  height: 34rpx;
  text-align: center;
  margin-left: 16rpx;
  border-radius: 22rpx;
  font-size: 20rpx;
  box-sizing: border-box;
  border: 1rpx solid rgba(153, 153, 153, 1);
}
.equity-btn {
  width: 92rpx;
  height: 46rpx;
  margin-right: 20rpx;
}
.equity-btn .btn {
  border: 1rpx solid #302E36 !important;
}