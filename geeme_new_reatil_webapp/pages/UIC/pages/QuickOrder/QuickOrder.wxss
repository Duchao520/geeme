@import '/utils/wxss/icon.wxss';

.QuickOrder{
  position: relative;
  background-color: #F8F8F8;
  height: 100vh;
  overflow: hidden;
}

.header-bg {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
  width: 900rpx;
  margin-left: -75rpx;
  height: 195rpx;
  background: #f52d56;
  border-radius: 50%/0 0 100% 100%;
}

.QuickOrder-container{
  margin: 78rpx 34rpx; 
  height:1010rpx;
  border-radius: 16rpx;
  overflow: hidden;
  background: white;
  padding-bottom: 100rpx;
  box-shadow: 0 0 20rpx 0 rgba(0, 0, 0, .2)
}

.userinfo{
  position:relative;
  height:200rpx;
  line-height:200rpx;
  color:#fff;
  background-size:100% 100%;
  background-image:url('data:image/png;base64,');
}

.userinfo-avatar{
  margin-left:40rpx;
  width:120rpx;
  height:120rpx;
  border-radius: 50%;
  vertical-align: middle;
}

.userinfo-text{
  margin-left:20rpx;
  max-width:300rpx;
  display: inline-block;
  vertical-align: middle;
  line-height: normal;
}

.userinfo-text > view:first-child{
  font-size:40rpx;
}

.userinfo-text > view:last-child{
  margin-top:10rpx;
  font-size: 32rpx;
}

.userinfo-level{
  position: absolute;
  line-height:normal;
  top:24rpx;
  right:40rpx;
  padding-left:42rpx;
  font-size:32rpx;
  background: no-repeat left center;
  background-size:37rpx 34rpx;
}

.qrcode-info{
  margin-top:80rpx;
  text-align: center;
  color:#999;
}

.qrcode-image{
  width:440rpx;
  height:440rpx;
}

.text-1{
  margin-top:40rpx;
  font-size:28rpx;
}

.text-2{
  margin-top:28rpx;
  font-size:32rpx;
}

.text-2 > text{
  color:#4A90E2;
}