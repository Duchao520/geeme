@import '/utils/wxss/icon.wxss';
/* 头部 */
.header {
  width: 100%;
  height: 316rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  position: relative;
}

.userInfo {
  width: 100%;
  height: 128rpx;
  padding-left: 38rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  position: absolute;
  left: 0;
  bottom: -75rpx;
}

.avatar {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  /* background-color: black; */
  border: 6rpx solid white;
  margin-right: 20rpx;
}

.userName {
  padding-top: 64rpx;
  font-size: 34rpx;
  color: #333;
  text-align: left;
}

/* 美丽档案主体 */

.archives {
  width: 100%;
  height: auto;
  margin-top: 144rpx;
  padding: 0 32rpx 100rpx;
}

.item {
  width: 100%;
  height: auto;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}

/* 左边 */

.i-left {
  width: 152rpx;
  height: auto;
  border-right: 2rpx solid #efeff4;
  padding-right: 24rpx;
  position: relative;
  padding-top: 16rpx;
}
.i-left.i-left-active{
  padding-top: 0;
}
.point {
  width: 12rpx;
  height: 12rpx;
  background-color:#EFEFF4;
  position: absolute;
  border-radius: 50%;
  top: 8rpx;
  right: -6rpx
}
.point.point-active{
  top: 25rpx;
}

.time {
  color: #333;
  font-size: 24rpx;
}

.shopInfo {
  margin-top: 10rpx;
}

.cityName {
  padding-left:25rpx;
  color: #999;
  font-size: 20rpx;
  white-space: nowrap;
  overflow:hidden;
  text-overflow: ellipsis;
  background: no-repeat left center;
  background-size:25rpx 25rpx;
}

.shopName {
  padding-left: 25rpx;
  margin-top: 8rpx;
  color: #999;
  font-size: 20rpx;
}

/* 右边 */

.i-right {
  width: 528rpx;
  padding-left: 16rpx;
  padding-top: 16rpx;
}
.i-right.i-left-active{
  padding-top: 0;
}

.images {
  width: 100%;
  font-size:0;
}

.imageItem {
  width: 164rpx;
  height: 164rpx;
  border-radius: 4rpx;
  margin-right: 9rpx;
}

.imageItem:last-child{
  margin-right:0;
}

.serviceList {
  width: 100%;
  height: auto;
  border-radius: 8rpx;
  background-color: #F8F8F8;
  padding: 16rpx;
  margin-top: 12rpx;
  margin-bottom:16rpx;
}

.serviceItem {
  width: 100%;
  line-height: 28rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  margin-top: 12rpx;
}

.serviceItem:first-child{
  margin-top:0;
}

.serviceName {
  color: #333333;
  font-size: 26rpx;
  white-space: nowrap;
  overflow:hidden;
  text-overflow: ellipsis;
}

.servicePrice {
    color: #F52D56;
  font-size: 26rpx;
}
