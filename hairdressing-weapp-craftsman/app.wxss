/**app.wxss**/
@import './utils/wxss/base.wxss';

view {
  box-sizing: border-box;
}

.button-hover {
  color: inherit;
  background-color: inherit;
}

button {
  position: relative;
  display: block;
  margin-left: auto;
  margin-right: auto;
  padding-left: inherit;
  padding-right: inherit;
  box-sizing: border-box;
  font-size: inherit;
  text-align: inherit;
  text-decoration: none;
  border-radius: inherit;
  -webkit-tap-highlight-color: transparent;
  overflow: hidden;
  color: inherit;
  background-color: inherit;
}

button::after {
  border: none;
}

input {
  min-height: inherit;
}

/* 底部按钮 */

.bottom-fixed {
  width: 100%;
  height: 140rpx;
  background-color: white;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2;
}

.bottom-button {
  width: 684rpx;
  height: 100rpx;
  border-radius: 50rpx;
  background-color: #f52d56;
  text-align: center;
  line-height: 100rpx;
  font-size: 34rpx;
  color: white;
  position: absolute;
  left: 34rpx;
  bottom: 20rpx;
}

.text--ellipsis_1 {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.text--ellipsis_2 {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.grey-space {
  width: 100%;
  height: 20rpx;
  background: #f8f8f8;
}

.moreIcon {
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.default-avatar {
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.arrow--right {
  background-image: url(data:image/png;base64,) !important;
}

.arrow--right_black_big{
  background-image:url('data:image/png;base64,') !important;
}

.icon--unSelected{
  background-image: url(data:image/png;base64,) !important;
}

.icon--selected{
  background-image: url(data:image/png;base64,) !important;
}

.icon--cantSelect{
  background-image:url('data:image/png;base64,') !important;
}

.app-icon-close {
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}


