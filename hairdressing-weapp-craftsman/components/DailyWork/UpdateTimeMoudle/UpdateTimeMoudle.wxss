/* 修改时间 */
.updateMoudle{
  width: 100%;
  height: auto;
  z-index: 999;
}
.updateHead{
  width: 100%;
  padding: 44rpx 38rpx 0;
}
.updateBody{
width: 100%;
height: auto;
padding: 0 32rpx;
margin-top: 68rpx;
display: flex;
flex-wrap: wrap;
justify-content: space-between;
padding-bottom: 240rpx;
}
.updateBodyItem{
  width: 148rpx;
  height: 70rpx;
  color: #999999;
  font-size: 24rpx;
  border: 1rpx solid #C1C0C9;
  border-radius: 4rpx;
  text-align: center;
  line-height: 70rpx;
  margin-bottom: 30rpx;
}
.updateBottom{
  width: 100%;
  height: 120rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  bottom: 0rpx;
  right: 0;
  background: white;
  border-top: 2rpx solid #EFEFF4;
  z-index: 999;
}
.updateBottom-left{
  flex: auto;
  padding-left: 32rpx;
  color: #333333;
  font-size: 28rpx;
}
.updateBottom-left text{
  color: #999999;
}
.updateBottom-right{
  width: 238rpx;
  height: 120rpx;
  color: white;
  font-size: 34rpx;
  background: #F52D56;
  text-align: center;
  line-height: 120rpx;
}
