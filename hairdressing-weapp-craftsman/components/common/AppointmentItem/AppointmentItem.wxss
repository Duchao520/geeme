.appointmentItem {
  width: 100%;
  min-height: 180rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}

/* 左侧时间 */

.appointmentItem-left {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-right: 20rpx;
}

.a-l-time {
  color: #999;
  font-size: 24rpx;
}

.appointmentItemStatus {
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
}

.appointmentItemStatus.EMPTY {
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.appointmentItemStatus.FULL {
  background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABQAAAAUBAMAAAB/pwA+AAAAGFBMVEUAAACMp/GMp/CLp/GMpvCNqPCOp/KLpvCLOWZDAAAAB3RSTlMA2/OlilVRb76TKwAAAFVJREFUCNdjAIJgIUVTBjBILAcCMRCLpRwMHIBMdwizhIGBrRwKEhgYYUwBhnAYs5TBHMYsZhCHMQsZ1GHMImQmQgGyNoRhyFYgLEY4B9WRCKcjPAQAB/1F//cifJUAAAAASUVORK5CYII=) no-repeat center center/100% auto;
}

.appointmentItemStatus.MIDDLE {
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.appointmentItem-decoration {
  width: 4rpx;
  flex: auto;
  background: #8ba6f0;
}

/* 右侧俩个人以上 */

.OutAppointmentItem-right {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  flex: auto;
}

.OutAppointmentItem-right.one .out-r-item {
  width: 600rpx;
  line-height: 140rpx;
}

.OutAppointmentItem-right.one .out-r-item > view {
  display: inline-block;
  vertical-align: middle;
  line-height: normal;
}

.out-r-item {
  width: 280rpx;
  height: 140rpx;
  border-radius: 8rpx;
  box-shadow: 0 0 16rpx 0 rgba(174, 184, 229, 0.3);
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
  padding-left: 18rpx;
  padding-right: 70rpx;
  margin-bottom: 40rpx;
  position: relative;
  overflow: hidden;
}

/* 右上角状态 默认待服务状态 */

.status {
  position: absolute;
  top: 0;
  right: 0;
  width: 70rpx;
  height: 70rpx;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}

.status.expired {
  background-image: url('data:image/png;base64,');
}

.status.finished {
  background-image: url('data:image/png;base64,');
}

.userName {
  color: #263238;
  font-size: 28rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.userMobile {
  color: #999;
  font-size: 24rpx;
}

.out-r-item.active {
  display: block;
  text-align: center;
  line-height:140rpx;
  padding: 0 40rpx;
}

.OutAppointmentItem-right.one .out-r-item.active {
  display: block;
  text-align: center;
}

.addCustomer {
  display: inline-block;
  vertical-align: middle;
  line-height: normal;
  width: 84rpx;
  height: 84rpx;
  border-radius: 8rpx;
}

.OutAppointmentItem-right.one .addCustomer {
  margin-right: 30rpx;
}

.addCustomer-top {
  width: 28rpx;
  height: 32rpx;
  background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABwAAAAcBAMAAACAI8KnAAAAFVBMVEUAAADY2NjY2NjY2Nja2trZ2dnY2Ni3KiqxAAAABnRSTlMAwOCxN/C46mh9AAAAM0lEQVQY02OAACcVBmRglozCFUscXFxnQRAISwVTJgxqaUggCY2LpniweAHGxR3siEgBAARuGaMuvzOdAAAAAElFTkSuQmCC) no-repeat center center/100% auto;
  margin: 8rpx auto 10rpx;
}

.addCustomer-bottom {
  text-align: center;
  color: #999;
  font-size: 24rpx;
}

.takeUp {
  display: inline-block;
  vertical-align: middle;
  line-height: normal;
  width: 84rpx;
  height: 84rpx;
  border-radius: 8rpx;
}

.takeUp-top {
  width: 28rpx;
  height: 32rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  margin: 9rpx auto 11rpx;
}

.takeUp-bottom {
  text-align: center;
  color: #999;
  font-size: 24rpx;
}

/* 没有用户，且占用 */

.NoCapacity {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #a7f1e0;
  background-image: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.NocapacityIcon {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 28rpx;
  height: 32rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

/* 过期 */

.out-r-item.out-time {
  background: #dbefff;
}
