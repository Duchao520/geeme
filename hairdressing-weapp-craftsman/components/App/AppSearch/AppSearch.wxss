.AppSearch{
  padding:0 32rpx;
  height:72rpx;
  line-height:72rpx;
}

.search-text{
  float:right;
  margin-left:20rpx;
  font-size:28rpx;
  color:#333;
}

.cart{
  float:right;
  margin-top:10rpx;
  margin-left:20rpx;
  width: 52rpx;
  height: 52rpx;
  background-image: url(data:image/png;base64,);
  background-size:100% 100%;
}

.cart.active{
  background-image: url(data:image/png;base64,);
}

.search-rect{
  position:relative;
  height:100%;
  background:#f8f8f8;
  font-size:28rpx;
  overflow:hidden;
}

.search-rect.SQUARE{
  border-radius:10rpx;
}

.search-rect.ROUND{
  border-radius:36rpx;
}

.search-selectType{
  float:left;
  color:#999;
  padding-left:28rpx;
}

.v-type{
  float:left;
  padding-right:30rpx;
  background:url('data:image/png;base64,') no-repeat right center;
  background-size:16rpx 10rpx;
}

.v-line{
  float:right;
  margin-left:8rpx;
  color:#c1c0c9;
}

.search-icon{
  float:left;
  margin-top:19rpx;
  margin-left:24rpx;
  width: 34rpx;
  height: 34rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  background-size: 100% 100%;
}

.search-input{
  padding-right:100rpx;
  height:100%;
  padding-left:20rpx;
  z-index:1;
}

.search-clearIcon{
  position:absolute;
  top:16rpx;
  right:20rpx;
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,);
  background-size:100% 100%;
  z-index: 2;
}

