.AppAvatar{
  position:relative;
  font-size:0;
}

.AppAvatar > image{
  width:100%;
  height:100%;
  border-radius:50%;
  background-size:100% 100%;
}

.icon{
  position: absolute;
  top:0;
  right:0;
  background-size:100% 100%;
}

