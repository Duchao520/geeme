.currentMonthResults{
  padding-bottom:150rpx;
}

/* 日报 */

.DayReport {
  width: 100%;
  height: auto;
  padding: 18rpx 32rpx;
}

/* 数据预览 */

.dayDataShow {
  width: 100%;
  height: 280rpx;
  margin-top: 44rpx;
  box-shadow: 0 2rpx 28rpx 0 rgba(174, 184, 229, 0.3);
}

.dayDataShowItem {
  position: relative;
  display: inline-block;
  width: 33.3%;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
}

.dayDataShowItem::after {
  content: '';
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -32rpx;
  width: 1px;
  height: 64rpx;
  background: #efeff4;
}

.dayDataShowItem:nth-child(3n)::after {
  content: '';
  display: none;
}

.dataItem-content {
  margin-top: 25rpx;
  display: inline-block;
  line-height: normal;
}

.dayShow-top {
  color: #333;
  font-size: 36rpx;
}

.dayShow-bottom {
  color: #999;
  font-size: 24rpx;
}

.dayShow-bottom.more{
  padding-right: 30rpx;
  background:url('data:image/png;base64,') no-repeat right center;
  background-size: 26rpx 26rpx;
}

/* 劳动业绩 */

.dayDataDetail {
  width: 100%;
  height: auto;
  margin-top: 60rpx;
}

.dayDataDetailTitle {
  color: #262628;
  font-size: 40rpx;
  text-align: left;
  font-weight: 600;
  margin-bottom: 40rpx;
  background: no-repeat right center;
  background-size: 14rpx 28rpx;
}

.dayDataDetailContent {
  width: 100%;
  height: auto;
}

.infoByDay-line{
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  font-size:26rpx;
  padding:16rpx 0;
  line-height: 1.5;
}

.infoByDay-line.title{
  color:#999;
  font-size:24rpx;
  background: #fff;
}

.infoByDay-line:nth-child(2n){
  background-color:#f8f8f8;
}

.infoByDay-cell{
  width: 100rpx;
}

.infoByDay-cell:first-child{
  padding-left:16rpx;
  width: 216rpx;
}

.infoByDay-cell.c9{
  color:#999;
}

.infoByDay-cell.red{
  color:#f52d56;
}

/* 月报 */

.MonthReport {
  width: 100%;
  height: auto;
  padding: 18rpx 32rpx;
}

/* 月报数据 */

.monthData {
  width: 100%;
  height: auto;
  margin-top: 60rpx;
}

.monthClomnName {
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  color: #999;
  font-size: 24rpx;
}

.monthClomnNameItem {
  width: 16.66%;
}

.monthClomnNameItem:first-child {
  padding-left: 16rpx;
}

.monthClomnContent {
  margin-top: 20rpx;
}

.monthLine {
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  color: #333;
  font-size: 26rpx;
  text-align: left;
}

.monthLine:nth-child(2n-1){
  background: #f8f8f8;
}

.monthLine > view{
  width: 16.66%;
}

.monthLine > view:first-child{
  color: #999;
}

/* 薪资明细 */

.PriceDetail {
  width: 100%;
  height: auto;
  padding: 18rpx 32rpx;
}

/* 排行榜 */

.RankList {
  width: 100%;
  height: auto;
  padding: 18rpx 32rpx;
}

.rankListBanner {
  width: 100%;
  height:80rpx;
  background-color:#f7f7f7;
  color: #666;
  font-size: 28rpx;
  padding:0 100rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}

.rankBannerItem.active {
  color:#f52d56;
}

.rankListContent {
  width: 100%;
  height: auto;
  margin-top: 40rpx;
}

.rankContentHead {
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
  color: #999;
  font-size: 24rpx;
}

.ranking {
  width: 77rpx;
  margin-left:30rpx;
  margin-right: 6rpx;
  font-size:32rpx;
}

.ranking-icon{
  width: 77rpx;
  height:40rpx;
  margin-left:30rpx;
  margin-right: 6rpx;
  background: no-repeat left center;
  background-size:30rpx 40rpx;
}

.ranking-icon.first{
  background-image: url(data:image/png;base64,);
}

.ranking-icon.second{
  background-image:url('data:image/png;base64,');
}

.ranking-icon.third{
  background-image:url('data:image/png;base64,');
}

.rankStaffInfo {
  width: 312rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
}

.results {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
  flex: auto;
  color: #333;
  font-size: 26rpx;
}

.resultsText {
  margin-right: 8rpx;
  padding-right:26rpx;
  background: url(data:image/png;base64,) no-repeat right center/100% auto;
  background-size:16rpx 12rpx;
}

.rankContentBody {
  width: 100%;
  height: auto;
}

.rankContentItem {
  width: 100%;
  height: 116rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}

.rankContentItem.active {
  background: #f8f8f8;
}

.rankStaffAvatar {
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.rankStaffName {
  color: #333;
  font-size: 28rpx;
}
