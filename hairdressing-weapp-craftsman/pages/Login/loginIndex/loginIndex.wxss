.loginIndex{
  width: 100%;
  height: auto;
  padding: 60rpx 72rpx 0;
}
.logo{
  width: 148rpx;
  height: 148rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}
.form{
  width: 100%;
  height: auto;
  margin-top: 50rpx;
}
input{
  width: 100%;
  height: 90rpx;
  font-size: 32rpx;
  border-bottom: 1px solid #EFEFF4;
}
.placeholder{
  color: #CCCCCC;
}
.loginButton{
  width: 100%;
  height: 100rpx;
  background: #5683FF;
  border-radius: 50rpx;
  box-shadow: 0 0 24rpx 0 rgba(112, 147, 243, .3);
  margin-top: 86rpx;
  color: white;
  font-size: 34rpx;
  text-align: center;
  line-height: 100rpx;
}
.stop-login{
  background: #cccccc;
}

.loginType{
  width: 100%;
  line-height: 34rpx;
  margin-top: 34rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}
.login-code{
  color: #4C7BFF;
  font-size: 24rpx;
}
.login-forget{
  color: #999999;
  font-size: 24rpx;
}
.choose{
  width: 28rpx;
  height: 28rpx;
  border-radius: 50%;
}
.border{
  border: 2rpx solid #AAAAAA;
}