/* 外部样式类 */
.nav-class{
  border-bottom: 2rpx solid #EBEBEB;
}

.releaseRelatedProduct{
  width: 100%;
  height: auto;
  padding-bottom: 140rpx;
}

/* 服务模块 */
.serviceMoudle{
  width: 100%;
  height: auto;
  padding-left: 32rpx;
  padding-bottom: 100rpx;
}
.serviceItem{
  width: 100%;
  height: auto;
  padding: 36rpx 0 32rpx;
  border-bottom: 2rpx solid #EFEFF4;
  position: relative;
  padding-left: 60rpx;
  padding-right: 32rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
}
.isSelectedIcon{
  width: 40rpx;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}
.selected-true{
  width: 40rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}
.selected-false{
  width: 40rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.service-left{
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  border-radius: 8rpx;
  margin-right: 24rpx;
  flex-shrink: 0;
}
.service-left image{
  width: 160rpx;
  height: 160rpx;
}
.service-right{
  flex: auto;
}
.serviceName{
  color: #333333;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.storeAddress{
  color: #999999;
  font-size: 24rpx;
  margin: 10rpx 0 10rpx;
  padding-left: 23rpx;
  position: relative;
}
.storeAddress::after{
  content: '';
  display: block;
  width: 18rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}
.servicePrice{
  color: #F52D56;
  font-size: 30rpx;
  font-weight: 600;
  line-height: 38rpx;
}
.servicePrice text{
  font-size: 20rpx;
}

/* 商品模块 */
.productMoudle{
  width: 100%;
  height: auto;
  padding-left: 32rpx;
  padding-bottom: 100rpx;
}

/* 底部按钮 */
.bottom-fixed{
  width: 100%;
  height: 140rpx;
  background-color: white;
  position: fixed;
  bottom: 0;
  left: 0;
}
.bottom-button{
  width: 684rpx;
  height: 100rpx;
  border-radius: 50rpx;
  background-color: #F52D56;
  text-align: center;
  line-height: 100rpx;
  font-size: 34rpx;
  color: white;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}