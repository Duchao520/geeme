/**app.wxss**/
@import './utils/wxss/base.wxss';
@import './utils/iconfont/iconfont.wxss';
view {
  box-sizing: border-box;
  word-break:break-all;
}

.button-hover {
  color: inherit;
  background-color: inherit;
}

button {
  position: relative;
  display: block;
  margin-left: auto;
  margin-right: auto;
  padding-left: inherit;
  padding-right: inherit;
  box-sizing: border-box;
  font-size: inherit;
  text-align: inherit;
  text-decoration: none;
  border-radius: inherit;
  -webkit-tap-highlight-color: transparent;
  overflow: hidden;
  color: inherit;
  background-color: inherit;
  line-height: inherit;
}

button::after {
  border: none;
}

input {
  min-height: inherit;
}

/* 弹框背景 */
.cover{
  position:fixed;
  height: 100%;
  left:0;
  top:0;
  width:100%;
  min-height:100vh;
  background: rgba(0, 0, 0, 0);
  overflow: hideden;
  z-index:-1;
  transition: all 0.625s ease;
}

.cover.active{
  background: rgba(0, 0, 0, .5);
  z-index:999;
}

/* 底部按钮 */
.bottom-fixed {
  width: 100%;
  height: 140rpx;
  background-color: white;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index:1;
}

.bottom-button {
  width: 684rpx;
  height: 100rpx;
  border-radius: 50rpx;
  background-color: #f52d56;
  text-align: center;
  line-height: 100rpx;
  font-size: 34rpx;
  color: white;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}

/* 包含底部很多按钮的父元素 
   使用页面:订单详情 等.
*/

.bottom-button-container {
  width: 100%;
  padding: 28rpx 0 68rpx;
  position: fixed;
  bottom: 0;
  right: 0;
  border-top: 1px solid #efeff4;
  z-index: 999;
  background: white;
}

.orderListButtons {
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-end;
  align-items: center;
}

.button-item {
  min-width: 150rpx;
  line-height: 54rpx;
  padding: 0 24rpx;
  font-size: 26rpx;
  text-align: center;
  border-radius: 50rpx;
  margin-right: 30rpx;
}

.button-item.red {
  color: #f52d56;
  border: 1rpx solid #f52d56;
}

.button-item.black {
  color: #666;
  border: 1rpx solid #d8d8d8;
}

.app-maskLayer {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 1000;
}

.app-icon-close {
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.text--ellipsis_1 {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.text--ellipsis_2 {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.fixed-top {
  position: fixed;
  top: 0;
  left: 0;
}

