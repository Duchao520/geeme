
.header {
  width: 100%;
  height: 224rpx;
  position: relative;
  overflow: hidden;
}
  /* 待服务 */
  .header.waitService{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 已服务(未支付) */
  .header.alreadyServiceNoPay{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 已服务(已支付) */
  .header.alreadyServicePay{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;    
  }
  /* 已超时 */
  .header.outTimeService{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 已取消 */
  .header.cancelService{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }


.status {
  width: 686rpx;
  /* height: 146rpx; */
  position: absolute;
  right: 0;
  left: 0;
  margin: auto;
  top: 20rpx;
  border-radius: 18rpx;
  padding-left: 124rpx;
}

.status-icon {
  width: 62rpx;
  height: 100%;
  position: absolute;
  left: 32rpx;
  top: 10rpx;
}
  /* 待服务 */
  .status-icon.waitService{
     background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 已服务(未支付) */
  .status-icon.alreadyServiceNoPay{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 已服务(已支付) */
  .status-icon.alreadyServicePay{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 已超时 */
  .status-icon.outTimeService{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 已取消 */
  .status-icon.cancelService{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }



.status-text {
  padding-top: 18rpx;
}

.s-t-paystatus {
  color: #fff;
  font-size: 32rpx;
}

.s-t-paytime {
  margin-top: 6rpx;
  color: #fff;
  font-size: 24rpx;
}