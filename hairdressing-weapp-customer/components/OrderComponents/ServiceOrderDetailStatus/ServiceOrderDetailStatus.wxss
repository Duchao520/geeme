
.header {
  width: 100%;
  height: 224rpx;
  position: relative;
  overflow: hidden;
}
  /* 待付款 */
  .header.WAITING_PAY{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 待服务 */
  .header.WAITING_SERVICE{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 已完成 */
  .header.COMPLETED{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;    
  }

  /* 待评价 */
  .header.WAITING_ASSESSMENT{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 已关闭 */
  .header.CLOSED{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }


.status {
  width: 686rpx;
  height: 102rpx;
  position: absolute;
  right: 0;
  left: 0;
  margin: auto;
  top: 20rpx;
  border-radius: 18rpx;
  padding-left: 124rpx;
}

.status-icon {
  width: 62rpx;
  height: 100%;
  position: absolute;
  left: 32rpx;
  top: 10rpx;
}
  /* 待付款 */
  .status-icon.WAITING_PAY{
     background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 待服务 */
  .status-icon.WAITING_SERVICE{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 已完成 */
  .status-icon.COMPLETED{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }
  /* 待评价 */
    .status-icon.WAITING_ASSESSMENT{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
    }

  /* 已关闭 */
  .status-icon.CLOSED{
    background: url(data:image/png;base64,) no-repeat center center/100% auto;
  }



.status-text {
  padding-top: 18rpx;
}
