/* components/Card/CouponModel/CouponModel.wxss */
@import '../../../utils/wxss/base.wxss';
.coupon{
  width: 100%;
  height: 260rpx;
  background-size: 100% 100%;
  padding: 46rpx 32rpx 16rpx;
}
.UN_USE{
  background-image: url(data:image/png;base64,)
}

.USED{
  background-image: url(data:image/png;base64,)
}

.INVALID{
  background-image: url(data:image/png;base64,)
}

.cover{
  position: fixed;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background: rgba(0, 0, 0, .5);
  z-index:100;
}

.popup-container{
  position: absolute;
  bottom:0;
  left:0;
  width:100%;
  background: #fff;
}

.popup-header{
  height:106rpx;
  line-height:106rpx;
  text-align: center;
  color:#333;
  font-size:34rpx;
  border-bottom:1px solid #efeff4;
}

.popup-header > .icon--close{
  position: absolute;
  top:34rpx;
  right:34rpx;
}

.popup-main{
  max-height: 800rpx;
  padding:0 32rpx 100rpx;
  overflow:scroll;
}

.main-header{
  height:100rpx;
  line-height:100rpx;
  color:#333;
  font-size:28rpx;
  font-weight: 600;
}

.main-line{
  height:80rpx;
  line-height:80rpx;
  font-size:26rpx;
  color:#333;
}

.main-line.title{
  height:60rpx;
  line-height:normal;
  color:#999;
  font-size:24rpx;
}

.main-line:nth-child(2n-1){
  background: #f8f8f8;
}

.main-line:nth-child(2n){
  background: #fff;
}

.main-line-left{
  display: inline-block;
  padding-left:16rpx;
  width:340rpx;
}

.main-line-right{
  display: inline-block;
}