
@import '../../../../template/CountShow.wxss';
/* 发型师信息 */

.container {
  width: 100%;
  height: auto;
  padding: 28rpx 32rpx 420rpx;
}

.container.fixed {
  position: fixed;
  top: 0;
  left: 0;
}

#moneyicon {
  font-size: 22rpx;
  display: inline-block;
  margin-right: -7rpx;
}

.moneyicon1 {
  font-size: 20rpx;
  display: inline-block;
  margin-right: -1rpx;
}

.header {
  width: 100%;
  height: 184rpx;
  padding: 24rpx 26rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  box-shadow: 0 0 8rpx 0 rgba(0, 0, 0, 0.1);
  position: relative;
  border-radius: 16rpx;
}

.hairavatar {
  width: 96rpx;
  height: 96rpx;
  background: no-repeat center center/100% auto;
  margin-right: 20rpx;
  border-radius: 50%;
}

.hairname {
  line-height: 48rpx;
  color: #263238;
  font-size: 34rpx;
}

.hairlevel {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
}

.hl-title {
  color: #333;
  line-height: 56rpx;
  font-size: 24rpx;
  margin-right: 12rpx;
}

.hl-store {
  width: 150rpx;
  height: 28rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  background-color: #f52d56;
}

.hairaddress {
  height: 32rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}

.ha-icon {
  width: 18rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  margin-right: 4rpx;
}

.ha-text {
  color: #999;
  font-size: 24rpx;
}

/* 已选服务信息 */

.packagelist {
  width: 100%;
  height: auto;
  margin-top: 40rpx;
}

.packageitem {
  width: 100%;
  height: auto;
  position: relative;
}

.pi-alt {
  color: #666;
  font-size: 24rpx;
  position: absolute;
  top: 12rpx;
  left: 18rpx;
}

/* copy */

.hs-body {
  border-radius: 8rpx;
  overflow: hidden;
}

.serviceitem {
  padding: 23rpx 0 34rpx 30rpx;
  background-color: #f8f8f8;
  border-top: 1rpx solid #EFEFF4;
}

.serviceitem:first-child {
  padding: 70rpx 0 34rpx 30rpx;
  border: 0;
}

.serviceitem-child {
  width: 100%;
  /* height: 226rpx; */
  position: relative;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}

.selected {
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  position: absolute;
  left: 34rpx;
  top: 0;
  bottom: 0;
  margin: auto;
}

.noselected {
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  /* background-color: black; */
  position: absolute;
  left: 34rpx;
  top: 0;
  bottom: 0;
  margin: auto;
  /* background-color: black; */
}

.counsts {
  position: absolute;
  top: 36rpx;
  right: 32rpx;
  text-align: right;
}

.serprice {
  width: auto;
  height: 36rpx;
  color: #f52d56;
  font-size: 30rpx;
}

.counst {
  color: #a3a3a3;
  font-size: 24rpx;
  margin-left: 20rpx;
}

.serimg {
  width: 160rpx;
  height: 160rpx;
  border-radius: 6rpx;
  background-color: black;
  margin-right: 24rpx;
}

.sercontext {
}

.serdes {
  width: 320rpx;
  max-height: 64rpx;
  color: #333;
  font-size: 28rpx;
  line-height: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: box;
  box-orient: vertical;
  line-clamp: 2;
}

.sershortdes {
  color: #999;
  font-size: 24rpx;
  line-height: 62rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}

.sershorttext {
  margin-right: 20rpx;
}

.sertime {
}

.seraddress {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
}

.sa-icon {
  width: 18rpx;
  height: 32rpx;
  margin-right: 4rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.sa-text {
  color: #aaa;
  font-size: 24rpx;
}

.totast {
  color: #f52d56;
  font-size: 24rpx;
  margin-top: 22rpx;
  text-align: left;
  margin-left: 94rpx;
}

/* 日历 */

.choose-time {
  width: 100%;
  height: auto;
}

.ct-head {
  width: 100%;
  height: 132rpx;
  padding: 14rpx 90rpx 0 100rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  position: relative;
}

.calendar{
  margin-top:56rpx;
  border-top: 1px solid #ebebeb;
}

.calendaricon {
  width: 80rpx;
  height: 80rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  position: absolute;
  background-size: 40rpx 40rpx;
  top: 8rpx;
  right: 0;
}

.cth-item {
  height: 100%;
  color: #666;
  text-align: center;
}

.cth-item.active {
  color: #f52d56;
}

.cth-text1 {
  width: 78rpx;
  display: block;
  font-size: 24rpx;
}

.cth-text2 {
  width: 78rpx;
  display: block;
  font-size: 28rpx;
  margin-top: 10rpx;
  color: #b9b9b9;
}

.cth-text2.active {
  width: 54rpx;
  display: block;
  font-size: 28rpx;
  margin-top: 10rpx;
  color: #f52d56;
}

.ct-body {
  width: 100%;
  height: auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.ctb-item {
  width: 148rpx;
  height: 70rpx;
  border-radius: 4rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 24rpx;
  color: #999;
  background: no-repeat center center;
  background-color: white;
  border: 1rpx solid rgba(221, 221, 221, 1);
  margin-bottom: 30rpx;
}

/* 埋伏好的类名 */

.ctb-item.outtime {
  background-color: #c1c0c9;
  color: white;
}

.ctb-item.outtimepeople {
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  color: white;
}

.ctb-item.outpeople {
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  color: white;
}

.ctb-item.normal {
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  color: white;
}

.ctb-item.outtime {
  background-color: #c1c0c9;
}

/* 备注 */

.note {
  width: 100%;
  height: auto;
  /* padding: 0 32rpx; */
  overflow: hidden;
  position: relative;
}

.notetitle {
  width: 100%;
  font-weight: 600;
  padding-top: 30rpx;
  padding-bottom: 20rpx;
  color: #262628;
  font-size: 30rpx;
  text-align: left;
}

.notearea {
  background-color: #f8f8f8;
  border-radius: 8rpx;
  overflow: hidden;
  position: relative;
}

.noteinput {
  width: 549rpx;
  line-height: 70rpx;
  background-color: #f8f8f8;
  color: #aaa;
  font-size: 28rpx;
  padding: 10rpx 28rpx;
}

.wordsnum {
  color: #aaa;
  font-size: 24rpx;
  position: absolute;
  right: 28rpx;
  bottom: 0;
  top: 0;
  margin: auto;
  line-height: 70rpx;
}

/* 提交预约 */

.submit {
  width: 100%;
  height: 120rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: white;
  padding-left: 32rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  border-top: 2rpx solid #efeff4;
  z-index: 999;
}

.submittime {
  line-height: 120rpx;
}

.des-word {
  color: #999;
  display: inline-block;
  margin-right: 12rpx;
  font-size: 28rpx;
}

.timeitem {
  color: #333;
  font-size: 28rpx;
}

.submitbtn {
  width: 240rpx;
  height: 100%;
  background-color: #f52d56;
  color: white;
  font-size: 34rpx;
  text-align: center;
  line-height: 120rpx;
}
.timeCalendarIcon{
   width: 80rpx;
  height: 80rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  background-size: 40rpx 40rpx;
}
