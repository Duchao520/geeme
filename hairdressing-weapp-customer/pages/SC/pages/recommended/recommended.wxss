.recommended-page{
  min-height:100vh;
}

.search-container{
  /* margin-bottom:40rpx; */
}

.historyshop {
  width: 718rpx;
  height: 440rpx;
  margin-left: 32rpx;
  margin-bottom: 54rpx;
}

.bg {
  width: 100%;
  height: calc(100vh - 100rpx);
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 100rpx;
  right: 0;
  z-index: 500;
  display: none;
}

.bg.active {
  display: block;
}

.historyshop-top {
  width: 100%;
  height: 56rpx;
  font-size: 37rpx;
  font-weight: 600;
  font-family: PingFangSC-Semibold;
  color: #262628;
  line-height: 56rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  padding-right: 32rpx;
  margin-bottom: 26rpx;
}

.history-text {
}

.history-iconbox {
  width: 60rpx;
  height: 100%;
  position: relative;
}

.history-icon {
  width: 14rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
}

.history-bottom {
  height: 356rpx;
  /* overflow: scroll; */
}

.history-items {
  /* width: 4000rpx; */
  height: 100%;
  margin-bottom: 10rpx;
  display: flex;
  /* padding: 29rpx 0 29rpx 24rpx; */
  box-sizing: border-box;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: nowrap;
  padding-bottom: 10rpx;
}

.historyshop-item {
  width: 422rpx;
  height: 338rpx;
  border-radius: 8rpx;
  overflow: hidden;
  background-color: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 0rpx 12rpx 0rpx rgba(0, 0, 0, 0.1);
  margin-right: 32rpx;
  flex-shrink: 0;
}

.history-img {
  width: 100%;
  height: 218rpx;
  background-color: aqua;
  /* background: url() no-repeat center center/100%; */
}

.historyshop-des {
  width: 100%;
  height: 120rpx;
  padding: 16rpx;
}

.historyshop-name {
  width: 192rpx;
  height: 44rpx;
  line-height: 44rpx;
  color: rgb(38, 38, 40);
  font-size: 32rpx;
  margin-bottom: 10rpx;
}

.historyshop-stars {
  width: 260rpx;
  height: 29rpx;
  line-height: 28rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}

.historyshop-star {
  width: 192rpx;
  height: 24rpx;
  background: no-repeat center center/100% auto;
  background-color: #b0bec5;
  background-image: url(data:image/png;base64,);
}

.historyshop-star.active {
  background-color: #f52d56;
}

.score {
  font-size: 24rpx;
  color: #f52d56;
}

.shopstores {
  /* width: 180rpx; */
  height: 28rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  margin: 16rpx 0 22rpx 0;
  align-items: center;
}

.shopstore {
  width: 142rpx;
  height: 27rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  /* background-color: #f52d56; */
}

.shopstore_copy {
  background-color: #f52d56;
  height: 27rpx;
}

.shopstore_bottom {
  width: 138rpx;
  height: 27rpx;
  background-color: #d3d3d3;
  margin-right: 10rpx;
}

.shopscore {
  font-size: 24rpx;
  color: #f52d56;
}

.body {
  position:relative;
  width: 100%;
  padding: 40rpx 32rpx 0;
}

.body.fixed {
  padding-top: 140rpx;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.pop-container {
  position: relative;
  width:100%;
  z-index:11;
}

.pop-container.fixed {
  position: fixed;
  top: 0;
  left: 0;
}

.popHeader-container {
  position: relative;
  width: 100%;
  border-top: 1px solid #efeff4;
  z-index: 11;
}

.popup-container {
  position: absolute;
  top: 100rpx;
  left: 0;
  width: 100%;
}

.cover {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 1;
}
