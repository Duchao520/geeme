.total-detail{
  width: 100%;  
  height: 150rpx;
  border-radius: 16rpx;
  box-sizing: border-box;
  padding: 28rpx 40rpx;
  background-color: rgba(255, 255, 255, 1);
  box-shadow: 0 4rpx 28rpx 0 rgba(174, 184, 229, 0.3);
  display: flex;

}
.total-detail .line{
  width: 2rpx;
  height: 62rpx;
  background: #EEEEEE;
}

.wallet .date-head{
  width: 100%;
  box-sizing: border-box;
  padding: 18rpx 32rpx;
  background: #F8F8F8;
  color: #666666;
}
.fc-999999{
  color: #999999;
}
.status{
  padding: 6rpx 12rpx;
  font-size: 20rpx;
  color: #FFFFFF;
  border-radius: 4rpx;
}
.status-wait{
  background: #F52D56;
}
.status-finish{
  background: #41CAC0;
}