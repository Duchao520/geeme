.dynamicDetail-page {
  padding-bottom: 200rpx;
}

.detail-item{
  margin-bottom:20rpx;
}

.detail-header {
  height: 120rpx;
}

.detail-header > .avatar {
  width: 80rpx;
  height: 80rpx;
  margin-left: 32rpx;
  margin-top: 20rpx;
  border-radius: 50%;
}

.header-info {
  display: inline-block;
  margin-left: 20rpx;
}

.header-info > .name {
  margin-top: 10rpx;
  font-size: 30rpx;
}

.header-info > .time {
  font-size: 24rpx;
  color: #9b9b9b;
}

.header-button {
  float: right;
  margin-top: 36rpx;
  margin-right: 32rpx;
  width: 122rpx;
  height: 48rpx;
  line-height: 48rpx;
  color: #f52d56;
  border: 1px solid #f52d56;
  border-radius: 30rpx;
  text-align: center;
  font-size: 28rpx;
}

.header-button.active{
  color:#999;
  border-color: #999;
}

.swiper-container{
  position:relative;
}

.swiper-indicator{
  position:absolute;
  left:32rpx;
  bottom:20rpx;
  width:80rpx;
  height:44rpx;
  line-height:44rpx;
  text-align: center;
  font-size:24rpx;
  color:#fff;
  background:rgba(0,0,0,0.2);
  border-radius:22rpx;
}

.detail-swiper {
  height:750rpx;
  background: #f4f4f4;
  display: flex;
  justify-content: center;
  align-items: center;
}

.detail-swiper.big{
  height:1000rpx;
}

.swiper-image{
  width:100%;
  height:100%;
}

.detail-main {
}

.main-text {
  padding: 0 32rpx;
  margin-top: 28rpx;
  font-size: 28rpx;
  color: #666;
}

.main-tags {
  padding: 0 32rpx;
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #f52d56;
}

.main-tags > text {
  margin-right: 10rpx;
}

.main-tags > text:last-child {
  margin: 0;
}

.related-swiper-item{
  box-sizing:border-box;
  padding:0 32rpx;
}

.related {
  margin-top: 20rpx;
  background: #f8f8f8;
  padding-left: 30rpx;
  padding-right: 20rpx;
  padding-bottom: 36rpx;
  padding: 0 20rpx 36rpx 30rpx;
}

.related-title {
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  color: #666;
}

.related-info {
  position: relative;
  padding-left: 180rpx;
}

.related-info > image {
  position: absolute;
  left: 0;
  top: 0;
  width: 160rpx;
  height: 160rpx;
}

.info-right {
  position: relative;
}

.info-right-1 {
  font-size: 28rpx;
  color: #333;
  height: 72rpx;
  line-height: 36rpx;
}

.info-right-2 {
  margin-top: 10rpx;
  padding-left: 25rpx;
  font-size: 24rpx;
  color: #999;

  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;

  background: url('data:image/png;base64,') no-repeat;
  background-position: left 7rpx;
  background-size: 25rpx 25rpx;
  vertical-align: bottom;
}

.info-right-3 {
  margin-top: 10rpx;
  font-size: 28rpx;
  color: #f52d56;
}

.info-right-btn {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 120rpx;
  height: 48rpx;
  line-height: 48rpx;
  background: #f52d56;
  border-radius: 24rpx;
  font-size: 26rpx;
  color: #fff;
  text-align: center;
}

.detail-actions-container {
  padding: 0 32rpx;
  height: 120rpx;
  color:#333;
  font-size:24rpx;
}

.action {
  display: inline-block;
  margin-top:40rpx;
  padding-left:50rpx;
  background: url() no-repeat left center;
  background-size: 40rpx 40rpx;
  min-width:40rpx;
  height:40rpx;
  line-height:40rpx;
}

.action.share{
  float:left;
  margin-left:32rpx;
  background-size:32rpx 38rpx;
  background-image:url('data:image/png;base64,');
}

.action.thumb{
  float:right;
  margin-right:40rpx;
  background-image:url('data:image/png;base64,');
}

.action.thumb.active{
  background-image:url('data:image/png;base64,');
}

.action.star {
  float:right;
  margin-right:40rpx;
  background-image: url('data:image/png;base64,');
}

.action.star.active{
  background-image:url('data:image/png;base64,');
}

.action.msg{
  float:right;
  margin-right:30rpx;
  background-image:url('data:image/png;base64,');
}

.detail-comments{
  margin:0 32rpx;
  padding:16rpx 20rpx 20rpx;
  background:#f7f7fa;
  font-size:24rpx;
}

.comments-item{
  margin-bottom:12rpx;
}

.comments-item-nickname{

}

.comments-item-content{
  color:#666;
}

.comments-showMore{
  color:#999;
}

.detail-addComment{
  position:relative;
  margin:0 32rpx;
  padding-left:50rpx;
  height:80rpx;
  line-height:80rpx;
}

.addComment-avatar{
  position:absolute;
  left:0;
  top:20rpx;
  width:40rpx;
  height:40rpx;
  border-radius:50%;
}

.addComment-input{
  width:100%;
  height:100%;
  display: inline-block;
  font-size:26rpx;
}

.pop-share {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 0;
  background: #fff;
  border-radius: 10rpx 10rpx 0 0;
  z-index: 1000;
  transition: height 0.5s ease;
}

.pop-share.active {
  height: 420rpx;
}

.share-title {
  position: relative;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 34rpx;
  color: #333;
}

.title-close {
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  width: 40rpx;
  height: 40rpx;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}

.share-button-container {
  line-height: 280rpx;
  text-align: center;
}

.share-button-item {
  display: inline-block;
  min-width: 100rpx;
  padding-top: 112rpx;
  line-height: normal;
  text-align: center;
  color: #333;
  font-size: 26rpx;
  background-position: center top;
  background-size: 100rpx 100rpx;
  background-repeat: no-repeat;
  vertical-align: middle;
}

.share-button-item.pic {
  margin-right: 190rpx;
  background-image: url('data:image/png;base64,');
}

.share-button-item.weixin {
  background-image: url('data:image/png;base64,');
}

.canvas{
  position: fixed;
  left:1000px;
  /* z-index: 2;
  left:0; */
  top:0;
  width:375px;
  height:667px;
}