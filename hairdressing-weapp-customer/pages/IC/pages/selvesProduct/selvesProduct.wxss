.selvesProduct{
width: 100%;
height: auto;

}

/* 头部 */
.selevesHeader{
  width: 100%;
  height: 380rpx;
  border-top: 1rpx solid #F8F8F8;
  padding-top: 76rpx;
}
.selvesIcon{
  width: 144rpx;
  height: 144rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  margin: 0 auto;
}
.selvesIconName{
  text-align: center;
  margin-top: 16rpx;
  color: #999999;
  font-size: 32rpx;
}

/* 可能喜欢 */
.selevesBody{
  width: 100%;
  height: auto;
  background: #F8F8F8;
}
.sellves-b-head{
  width: 100%;
  /* height: 30rpx; */
  padding: 40rpx 0rpx 0rpx 30rpx;

}
.selleves-b-headItem{
  padding-left: 18rpx;
  border-left: 6rpx solid #F52D56;
  color: #262628;
  font-size: 32rpx;
}
.selves-b-content{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
