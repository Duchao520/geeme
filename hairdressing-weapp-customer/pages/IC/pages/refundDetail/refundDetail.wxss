.gray-line{
  background:#f8f8f8;
  width:120%;
  height:20rpx;
  margin-left:-32rpx;
}

.refundDetail-page{
  padding:0 32rpx 200rpx;
}

.refundDetail-header{
  padding:15rpx 0;
  line-height:50rpx;
  font-size:12px;
  color:#333333;
}

.refundDetail-header text{
  color:#999999;
}

.timeLine-container{
  padding-top:40rpx;
}

.timeLine{
  position:relative;
  padding-left:60rpx;
  height:120rpx;
  line-height:40rpx;
  font-size:14px;
  color:#333;
}

.timeLine.reject{
  height:160rpx;
}

.icon{
  position:absolute;
  left:0;
  top:0;
  width:44rpx;
  height:44rpx;
}

.icon.right{
  background:url('data:image/png;base64,') no-repeat center center/100% auto;
}

.icon.wait{
  background:url('data:image/png;base64,') no-repeat center center/100% auto;
}

.icon.reject{
  background:url('data:image/png;base64,') no-repeat center center/100% auto;
}

.icon::after{
  content:'';
  position:absolute;
  left:21rpx;
  top:44rpx;
  width:2px;
  height:80rpx;
  background:url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAABQBAMAAADMwMmsAAAAD1BMVEUAAAC1vce2vcm0vMa0vMZ9XZpNAAAABHRSTlMAgELlQIlGagAAABZJREFUCNdjYDJiYHREQyBBJDCc1AAAY1oNraXFMjkAAAAASUVORK5CYII=') no-repeat center center/100% auto;
}

.timeLine:last-child > .icon::after{
  background:none;
}

.icon.reject::after{
  height:120rpx;
}

.timeLine > .time {
  font-size:12px;
  color:#999;
}

.refundMoney-container{
  color:#333;
  font-size:28rpx;
}

.refundMoney{
  height:100rpx;
  line-height:100rpx;
  border-bottom:1px solid #efeff4;
}

.refundMoney:last-child{
  border:none
}

.refundMoney text{
  margin-left:30rpx;
}

.message-header{
  height:100rpx;
  line-height:100rpx;
  font-size:34rpx;
  border-bottom:1px solid #efeff4;
}

.message-header > .header-button{
  float:right;
  margin-top:35rpx;
  width:132rpx;
  height:40rpx;
  line-height:40rpx;
  text-align: center;
  border:0.5px solid #d8d8d8;
  font-size:26rpx;
  color:#666;
}

.message-h2{
  height:100rpx;
  line-height:120rpx;
  font-size:30rpx;
}

.customer-msgContainer{
  padding:20rpx;
  border:1px solid #efeff4;
}

.saler-msgContainer{
  position:relative;
  padding:20rpx;
  background:#f7f7fa;
}

.arrow1{
  position: absolute;
  left:40rpx;
  top:-10rpx;
  width:0;
  height:0;
  border-width:0 16rpx 10rpx;
  border-style:solid;
  border-color:transparent transparent #f7f7fa;
}

.msgContainer-time{
  color:#9b9b9b;
  font-size:24rpx;
}

.msgContainer-text{
  margin-top:20rpx;
  font-size:28rpx;
}

.msgContainer-image{
  margin-top:20rpx;
}

.msgContainer-image > image{
  width:200rpx;
  height:200rpx;
  margin-right:20rpx;
}

.msgContainer-image > image:last-child{
  margin:0;
}

.leaveMessage-container{
  position:fixed;
  bottom:0;
  width:100%;
  height:0;
  z-index:1001;
  background:#fff;
  transition:all .5s ease;
}

.leaveMessage-container.active{
  height:822rpx;
}

.leaveMessage-header{
  position:relative;
  height:100rpx;
  line-height:100rpx;
  text-align: center;
  font-size:34rpx;
  border-bottom:1px solid #efeff4;
}

.app-icon-close.refundDetail{
  position:absolute;
  width:30rpx;
  height:30rpx;
  top:35rpx;
  right:30rpx;
}

.leaveMessage-message{
  position:relative;
  margin-top:34rpx;
}

.leaveMessage-message > textarea{
  font-size:28rpx;
  width:650rpx;
  height:240rpx;
  margin:0 auto;
  background:#f8f8f8;
  padding:20rpx;
}

.message-length{
  position:absolute;
  right:45rpx;
  bottom:14rpx;
  font-size:24rpx;
  color:#aaa;
}

.updatePic{
  width: 100%;
  padding: 34rpx;
}

.picItem{
  display: inline-block;
  width:180rpx;
  height:180rpx;
  margin-right:40rpx;
}

.picItem image{
  width:100%;
  height:100%;
}

.updatePicItem{
  display: inline-block;
  width: 180rpx;
  height: 180rpx;
  /* border: 2rpx solid #C8C7CC; */
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.leaveMessage-bottom-button{
  width: 684rpx;
  height: 100rpx;
  border-radius: 50rpx;
  background-color: #F52D56;
  text-align: center;
  line-height: 100rpx;
  font-size: 34rpx;
  color: white;
  margin:0 auto 50rpx;
}