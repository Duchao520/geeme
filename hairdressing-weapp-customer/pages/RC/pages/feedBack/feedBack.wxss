.feedBack{
  width: 100%;
  height: auto;
  min-height: #F8F8F8;
  background: black;
  position: relative;
}
.feedBackContent{
  width: 100%;
  height: auto;
  position: absolute;
  right: 0;
  top: 346rpx;
  box-shadow: 0 -4rpx 8rpx 0 rgba(174, 184, 229, 0.3);
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
  overflow: hidden;
}
/* 问题类型 */
.problemType{
  width: 100%;
  height: 94rpx;
  padding-left: 32rpx;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: flex-start;
  position: relative;
}
.picker{
  flex: auto;
}
.problemType-left{
  color: #333333;
  font-size: 28rpx;
}
.problemType-right{
  width: 80%;
  color: #999999;
  font-size: 28rpx;
  margin-left: 32rpx;
  flex: auto;
}
.moreIcon{
  width: 12rpx;
  height: 94rpx;
  position: absolute;
  right: 34rpx;
  top: 0;
  bottom: 0;
  margin: auto;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}





/* 上传主体 */
.uploadMoudle{
  padding: 20rpx 0;
  background: #F8F8F8;
}
.uploadItem{
  width: 100%;
  height: auto;
  padding: 30rpx 32rpx;
  background: white;
}
.inputItem{
  width: 100%;
  position: relative;
  height: 80rpx;
  border-radius: 50rpx;
  background: #F8F8F8;
}
.inputItem input{
  width: 88%;
  height: 80rpx;
  color: #AAAAAA;
  font-size: 28rpx;
  padding-left: 28rpx;
  padding-right: 30rpx;
}
.wordLength{
  position: absolute;
  right: 28rpx;
  top: 0;
  bottom: 0;
  margin: auto;
  color: #AAAAAA;
  font-size: 24rpx;
  line-height: 80rpx;
  background: #F8F8F8;
  z-index: 999;
}
.chooseImage{
  width: 180rpx;
  height: 180rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  margin-top: 40rpx;
}

/* 联系电话 */
.userPhone{
  width: 100%;
  height: 94rpx;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: flex-start;
  padding-left: 32rpx;
}
.userTitle{
  color: #333333;
  font-size: 28rpx;
  margin-right: 32rpx;
}
.userPhone input{
  flex: auto;
  height: 94rpx;
  color: #999999;
  font-size: 28rpx;
}

/* 提示 */
.prompt{
  padding-top: 34rpx;
  padding-bottom: 290rpx;
  color: #999999;
  font-size: 24rpx;
  text-align: left;
  padding-left: 34rpx;
  background: #F8F8F8;
}

/* 提交反馈 */
.submitfeedBack{
  width: 100%;
  padding: 20rpx 0 40rpx;
  position: fixed;
  bottom: 0;
  right: 0;
  background: white;
}
.submitfeedBackitem{
  width: 684rpx;
  height: 100rpx;
  color: white;
  font-size: 34rpx;
  border-radius: 50rpx;
  background: #F52D56;
  line-height: 100rpx;
  text-align: center;
  margin: 0 auto;
}