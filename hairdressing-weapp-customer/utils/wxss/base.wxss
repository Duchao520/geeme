/*toast提示语*/
.common-toast {
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 11000;
  width: auto;
  padding: 0 8px;
  line-height: 34px;
  border-radius: 5px;
  text-align: center;
  font-size: 14px;
  color: white;
  background: rgba(0, 0, 0, 0.7);
  display: none;
  z-index: 3000;
}

.common-toast-show {
  -webkit-animation: commonToastShow ease 300ms;
  animation: commonToastShow ease 300ms;
}

.common-toast-hide {
  -webkit-animation: commonToastHide ease 500ms;
  animation: commonToastHide ease 500ms;
}

@-webkit-keyframes commonToastShow {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, 0, 0) scale(0);
    transform: translate3d(0, 0, 0) scale(0);
  }
  100% {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0) scale(1);
    transform: translate3d(0, 0, 0) scale(1);
  }
}

@keyframes commonToastShow {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, 0, 0) scale(0);
    transform: translate3d(0, 0, 0) scale(0);
  }
  100% {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0) scale(1);
    transform: translate3d(0, 0, 0) scale(1);
  }
}

@-webkit-keyframes commonToastHide {
  0% {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0) scale(1);
    transform: translate3d(0, 0, 0) scale(1);
  }
  100% {
    opacity: 0;
    -webkit-transform: translate3d(0, 0, 0) scale(0);
    transform: translate3d(0, 0, 0) scale(0);
  }
}

@keyframes commonToastHide {
  0% {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0) scale(1);
    transform: translate3d(0, 0, 0) scale(1);
  }
  100% {
    opacity: 0;
    -webkit-transform: translate3d(0, 0, 0) scale(0);
    transform: translate3d(0, 0, 0) scale(0);
  }
}

/*loading*/
.loading {
  width: 30px;
  height: 30px;
  background: url(data:image/gif;base64,) no-repeat center;
  background-size: cover;
  animation: loading 3s linear infinite;
  display: block;
  margin: 0 auto;
  z-index: 99;
}

@keyframes loading {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}

.fade-enter-active{
  animation: fadeInUp .3s;
}
.fade-leave-active{
  animation: fadeOutDown .3s;
}





/*单行文本溢出*/
.txt-ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/*双行文本溢出*/
.txt-overflow {
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}


/*遮罩层*/
.mask {
  background: rgba(0, 0, 0, .5);
  position: fixed;
  z-index: 5;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
}

/*弹出框样式*/
.alert-box {
  position: fixed;
  padding-top: 37px;
  width: 78%;
  left: 11%;
  top: 50%;
  z-index: 7;
  text-align: center;
  border-radius: 4px;
  transform: translateY(-50%);
}

.close-btn {
  position: absolute;
  width: 27px;
  height: 27px;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: cover;
  top: 240rpx;
  right: -100rpx;
}


/*单选框*/
.radio-box {
  width: auto;
  position: relative;
  display: inline-block;
  margin-left: 50rpx;
}

.radio-box input[type='radio'] {
  opacity: 0;
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 2;
}

.radio-box span {
  display: block;
  position: relative;
  padding-left: 100rpx;
}

.radio-box span:before {
  content: '';
  display: block;
  position: absolute;
  width: 15px;
  height: 15px;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: cover;
  border-radius: 50%;
  left: 0;
  top: 14px;
}

input[type='radio']:checked + span:before {
  background: url(data:image/png;base64,) no-repeat center;
  background-size: cover;
}

.alertMsg-enter-active {
  animation: zoomIn .3s;
}

.alertMsg-leave-active {
  animation: zoomOut .3s;
}

