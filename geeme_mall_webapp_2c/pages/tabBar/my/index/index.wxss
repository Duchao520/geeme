@import '/utils/wxss/icon.wxss';
/* 用户头部信息 */
page{
  position:relative;
  min-height: 100vh;
}

.header {
  overflow: hidden;
  margin:0 32rpx;
  padding-bottom:30rpx;
  border-radius:20rpx 20rpx 0 0;
  color: white;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:cover;
}

.header.notLogin {
  background-image: url(data:image/png;base64,);
}

.header-top {
  margin-top: 40rpx;
  margin-bottom: 10rpx;
  overflow: hidden;
}

.top-avatar {
  float: left;
  margin-left: 40rpx;
  margin-right: 20rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.top-right {
  position:relative;
  display: block;
  line-height: 120rpx;
  overflow: hidden;
}

.btn-getUserInfo{
  position:absolute;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background:transparent;
  z-index:10;
}

.top-text {
  display: inline-block;
  width: 100%;
  line-height: normal;
  vertical-align: middle;
}

.top-nickname {
  font-size: 36rpx;
}

.top-nickname > text {
  display: inline-block;
  padding-right: 30rpx;
  background: no-repeat right 18rpx;
  background-size: 24rpx 24rpx;
}

.top-nickname > text.man {
  background-image: url(data:image/png;base64,);
}

.top-nickname > text.woman {
  background-image: url(data:image/png;base64,);
}

.top-arrow {
  float: right;
  margin-top: 11rpx;
  margin-right: 22rpx;
  width: 15rpx;
  height: 26rpx;
  background: no-repeat center center/100% auto;
  background-size: 100% 100%;
}

.top-memberId {
  margin-top: 10rpx;
  font-size: 24rpx;
}

.header-bottom {
  padding-left: 40rpx;
  margin-top: 20rpx;
  font-size: 24rpx;
  overflow: hidden;
  line-height: 52rpx;
}

.bottom-level {
  float: right;
  width: 180rpx;
  height: 52rpx;
  text-align: center;
  border-radius:26rpx 0 0 26rpx;
  background: rgba(255, 255, 255, .2);
}

.bottom-level > text{
  display: inline-block;
  background: no-repeat left center;
  background-size:30rpx 26rpx;
  padding-left:34rpx;
}

.funcmodel {
  width: 100%;
  height: 212rpx;
  line-height: 212rpx;
  text-align: center;
  box-shadow: 0 0 8rpx 0 rgba(174, 184, 229, 0.3);
}

.func-item {
  display: inline-block;
  min-width:104rpx;
  margin-right:50rpx;
  line-height:normal;
  vertical-align: middle;
  font-size:0;
}

.func-item:last-child{
  margin:0;
}

.item-img {
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
  margin-bottom: 26rpx;
}

.item-name {
  color: rgba(51, 51, 51, 1);
  font-size: 26rpx;
  text-align: center;
}

/* 我的预约 */

.appointment {
  width: 100%;
  height: auto;
  margin-top: 56rpx;
}

.appointment-head {
  width: 100%;
  height: 44rpx;
  margin-bottom: 14rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  padding: 0 34rpx 0 42rpx;
}

.ah-left {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}

.al-icon {
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  margin-right: 12rpx;
}

.al-text {
  color: rgba(79, 80, 84, 1);
  font-size: 32rpx;
  line-height: 44rpx;
  font-weight: 500;
}

.ah-right {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-end;
}

.ar-icon {
  width: 11rpx;
  background: no-repeat center center/100% auto;
}

.ar-text {
  color: rgba(153, 153, 153, 1);
  font-size: 24rpx;
  margin-right: 12rpx;
  line-height: 44rpx;
}

.appointment-body {
  width: 100%;
  height: auto;
  padding: 28rpx;
}

.appointment-item {
  width: 100%;
  box-shadow: 0 0 28rpx 0px rgba(174, 184, 229, 0.3);
  border-radius: 16rpx;
  padding: 24rpx 20rpx 48rpx 24rpx;
}

.ap-top {
  position:relative;
  width: 100%;
  height: 164rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}

.headinfo {
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}

.headavatar {
  width: 94rpx;
  height: 94rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}

.headname {
  color: rgba(38, 50, 56, 1);
  font-size: 34rpx;
  margin-bottom: 10rpx;
}

.headposition {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}

.headposition-text {
  color: rgba(51, 51, 51, 1);
  font-size: 24rpx;
  margin-right: 30rpx;
}

.headposition-address {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
}

.ha-icon {
  width: 18rpx;
  height: 24rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  margin-right: 4rpx;
}

.ha-text {
  color: rgba(153, 153, 153, 1);
  font-size: 24rpx;
}

.headtime {
  width: 100%;
  height: 34rpx;
  margin-top: 10rpx;
  font-size: 24rpx;
  color: #999;
}

.headstatus {
  width: 92rpx;
  height: 38rpx;
  background-color: #58c9f3;
  color: white;
  font-size: 24rpx;
  text-align: center;
  line-height: 38rpx;
  border-radius: 4rpx;
}

.ap-bottom {
  width: 100%;
  padding: 22rpx;
  background-color: rgba(248, 248, 248, 1);
  border-radius: 8rpx;
}

.ap-item:first-child {
  margin-top: 0;
}

.ap-item {
  width: 100%;
  height: 34rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  margin-top: 16rpx;
}

.ap-species {
  width: 412rpx;
  line-height: 34rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}

.species {
  width:200rpx;
  color: rgba(51, 51, 51, 1);
  font-size: 24rpx;
}

.speciesname {
  width:200rpx;
  margin-left: 20rpx;
  color: #999;
  font-size: 24rpx;
}

.speciesname > text:first-child{
  max-width:150rpx;
  float:left;
}

.speciesname > text:last-child{
  float:right;
}

.speciesprice {
  color: #f52d56;
  font-size: 24rpx;
}

/* 我的订单 */

.myorder {
  width: 100%;
  height: auto;
  margin-top: 33rpx;
}

.myorder-head {
  width: 100%;
  height: 44rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  padding: 0 34rpx 0 42rpx;
}

.mh-left {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}

.mh-icon {
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  margin-right: 12rpx;
}

.mh-icon1{
  width: 40rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  margin-right: 18rpx;
}

.mh-text {
  color: rgba(79, 80, 84, 1);
  font-size: 32rpx;
  line-height: 44rpx;
  font-weight: 500;
}

.mh-right {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-end;
}

.mr-text {
  color: rgba(153, 153, 153, 1);
  font-size: 24rpx;
  margin-right: 12rpx;
  line-height: 44rpx;
}

.mr-icon {
  width: 11rpx;
  background: no-repeat center center/100% auto;
}

.myorder-body {
  width: 100%;
  height: 300rpx;
  padding: 28rpx;
}

.myorder-item {
  width: 100%;
  text-align: center;
  font-size:0;
}

.order-img {
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto 30rpx;
}

.order-text {
  color: rgba(51, 51, 51, 1);
  font-size: 24rpx;
  text-align: center;
  line-height: 24rpx;
}

.myorder-items {
  width: 100%;
  height: 100%;
  box-shadow: 0 0 28rpx 0px rgba(174, 184, 229, 0.3);
  border-radius: 16rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}

