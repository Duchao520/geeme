@import '/template/OrderPreference/OrderPreference.wxss';
.orderDetail {
  position: relative;
  background-color: white;
  padding-bottom: 280rpx;
  z-index: 1;
}

/* 已约时间 */

.body {
  width: 100%;
  height: auto;
  padding: 0 32rpx 40rpx;
  background-color: white;
}

/* 发型师 */

.staff {
  width: 100%;
  /* height: 184rpx; */
  border-radius: 16rpx;
  box-shadow: 0 0 8rpx 0 rgba(0, 0, 0, 0.1);
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  padding: 24rpx 0 32rpx 26rpx;
  position: relative;
}

.avatar {
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.s-info {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 10rpx;
}

.s-name {
  color: #263238;
  font-size: 34rpx;
  line-height: 48rpx;
  margin-right: 4rpx;
}

.s-level {
  color: #333;
  font-size: 24rpx;
}

.s-shopinfo {
  color: #999;
  font-size: 24rpx;
  margin-bottom: 6rpx;
}

.s-address {
  width: 400rpx;
  overflow: hidden;
  display: box;
  box-orient: vertical;
  line-clamp: 1;
  color: #999;
  font-size: 24rpx;
}

.staffbutton {
  width: 100rpx;
  height: 36rpx;
  border-radius: 22rpx;
  border: 1px solid #f52d56;
  color: #f52d56;
  font-size: 20rpx;
  text-align: center;
  line-height: 30rpx;
  position: absolute;
  right: 20rpx;
  bottom: 16rpx;
}

.noStaff {
  width: 100%;
  border-radius: 16rpx;
  box-shadow: 0 0 8rpx 0 rgba(0, 0, 0, 0.1);
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  padding: 18rpx 20rpx;
}

.noStaff-left {
  font-size: 24rpx;
  color: #666;
}

.noStaff-right {
  width: 100rpx;
  height: 36rpx;
  border-radius: 22rpx;
  border: 1px solid #f52d56;
  color: #f52d56;
  font-size: 20rpx;
  text-align: center;
  line-height: 30rpx;
}

.phone{
  width: 100rpx;
  height: 100rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  background-size: 32rpx 32rpx;
  position: absolute;
  top: 44rpx;
  right: 20rpx;
}

/* 已选服务 */

.service {
  width: 100%;
  height: auto;
  border-radius: 8rpx;
  background-color: #f8f8f8;
  padding-top: 22rpx;
  position: relative;
}
.service.active{
  margin-top: 40rpx;
}

.service-title {
  width: 100%;
  height: 36rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  padding: 0 18rpx;
}

.service-text {
  color: #666;
  font-size: 24rpx;
}

.service-button {
  width: 100rpx;
  height: 36rpx;
  border-radius: 22rpx;
  border: 1px solid #f52d56;
  color: #f52d56;
  font-size: 20rpx;
  text-align: center;
  line-height: 30rpx;
}

.service-item {
  width: 100%;
  /* height: 160rpx; */
  padding: 24rpx 10rpx 24rpx 30rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}

.serviceimage {
  width: 160rpx;
  height: 160rpx;
  border-radius: 6rpx;
}

.serviceinfo {
}

.servicedes {
  width: 458rpx;
  height: 72rpx;
  color: #333;
  font-size: 28rpx;
  line-height: 36rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: box;
  box-orient: vertical;
  line-clamp: 2;
  margin-bottom: 10rpx;
}

.packageinfo {
  color: #7e7e7e;
  font-size: 24rpx;
  margin-bottom: 6rpx;
}

.price {
  display: inline-block;
  color: #f52d56;
  font-size: 30rpx;
  font-weight: 600;
  margin-right: 20rpx;
}

.count {
  display: inline-block;
  color: #7e7e7e;
  font-size: 24rpx;
}

/* 订单信息 */

.orderInfo {
  width: 100%;
  padding-bottom: 20rpx;
  background-color: #F8F8F8;
}

.orderInfo-Item {
  background-color: white;
  padding-left: 32rpx;
}

.orderHeader {
  position:relative;
  width: 100%;
  padding: 16rpx 0;
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
}

.copyButton{
  color: #666666;
  font-size: 26rpx;
  border: 1px solid #D8D8D8;
  line-height: 40rpx;
  text-align: center;
  padding: 0 14rpx;
  position: absolute;
  right: 32rpx;
  top: 12rpx;
}

.orderCode {
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  font-size: 24rpx;
  margin-top: 16rpx;
}
.orderCode:first-child{
  margin-top: 0;
}

.orderCode-text {
  color: #333;
}

.orderCode-number {
  font-size: 24rpx;
  color: #999;
}

.orderBody {
  width: 100%;
  padding: 16rpx 0;
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
  border-top: 2rpx solid #efeff4;
  border-bottom: 2rpx solid #efeff4;
}

.orderBody-appointmentTime {
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}

.o-a-l {
  color: #333;
  font-size: 24rpx;
}

.o-a-r {
  color: #f52d56;
  font-size: 24rpx;
}

.notes {
  width: 100%;
  height: 66rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
}

.notes-l {
  color: #333;
  font-size: 24rpx;
}

.notes-r {
  color: #999;
  font-size: 24rpx;
}

/* 按钮 */
.buttons {
  width: 100%;
  height: 120rpx;
  position: fixed;
  bottom: 0;
  right: 0;
  background-color: white;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-end;
  align-items: center;
  border-top: 2rpx solid #efeff4;
  z-index: 999;
  font-size: 26rpx;
}

.buttons > .btn{
  border-radius: 26rpx;
  border: .5px solid #d8d8d8;
  margin-right: 32rpx;
  width:156rpx;
  height:54rpx;
  line-height:54rpx;
  text-align: center;
}

.buttons > .btn.red{
  border: 0.5px solid #f52d56;
  color: #f52d56;
}

.productSpecification{
  padding: 0 14rpx;
  line-height: 36rpx;
  color: #666666;
  font-size: 26rpx;
  border: 1rpx solid rgba(216, 216, 216, 1);
  position: absolute;
  right: 32rpx;
  bottom: 32rpx;
  border-radius: 50rpx;
  background: white;
}

.evaluationModule{
  background: #f8f8f8;
  padding-bottom:20rpx;
}

.evaluationContent{
  padding:0 32rpx;
  height:80rpx;
  line-height:80rpx;
  font-size:24rpx;
  color:#333;
  background:#fff;
}

.evaluationContent > .arrow--right{
  width:16rpx;
  height:100%;
  float:right;
  background:no-repeat center center;
  background-size:12rpx 18rpx;
}

.groupBuy-left{
  display: inline-block;
  margin-left:32rpx;
  color:#333;
  font-size:24rpx;
}