/* pages/TC/appointmentsuccess/appointmentsuccess.wxss */
.container{
  width: 100%;
  height: auto;
  margin: 150rpx auto 570rpx;
}
.successicon{
  width: 220rpx;
  height: 220rpx; 
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  margin: 0 auto;
}
.alttitle{
  margin: 34rpx auto 52rpx;
  text-align: center;
}
.altitle-one{
  line-height: 48rpx;
  color: #333333;
  font-size: 32rpx;
  margin-bottom: 6rpx;
}
.altitle-two{
  line-height: 48rpx;
  color: #999999;
  font-size: 28rpx;
}
.packageinfo{
  width: 100%;
  height: auto;
}
.package-item{
  text-align: center;
  color: #333333;
  font-size: 28rpx;
  line-height: 48rpx;
  padding: 0 64rpx;
}
.package-item text{
  max-width: 232rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: box;
  box-orient: vertical;
  line-clamp: 1;
  margin-right: 26rpx;
}

.preferential{
  text-align: center;
  line-height: 48rpx;
  color: #B8B8B8;
  font-size: 28rpx;
}
.buttons{
  width: 100%;
  height: 80rpx;
  padding: 0 128rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  margin-top: 84rpx;
}
.paying{
  width: 684rpx;
  height: 100rpx;
  background-color: #F62B5D;
  border-radius: 50rpx;
  text-align: center;
  line-height: 100rpx;
  color: white;
  font-size: 34rpx;
  position: fixed;
  bottom: 40rpx;
  right: 0;
  left: 0;
  margin: auto;
}
.showappointment{
    width: 220rpx;
  height: 100%;
  border:2rpx solid #F52D56;
  color: #F62B5D;
  border-radius: 40rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;

}