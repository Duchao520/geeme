.vipCard{
  width: 100%;
  position: relative;
}

swiper{
  height: 400rpx;
  margin-bottom: 38rpx;
  margin-top: 32rpx;
}

.card {
  display: inline-block;
  text-align: left;
  margin: 0 auto;
  position: relative;
  width: 630rpx;
  height: 400rpx;
  border-radius: 20rpx;
  background-size: 100% 100%;
  color: white;
  z-index: 10;
  overflow: hidden;
  transition: all .625s ease;
  transform: scale(1,0.8);
}

.card.buy{
  background: linear-gradient(#f52d56, #f86094);
  text-align: center;
  border-radius:20rpx;
  color:#fff;
  font-size:24rpx;
  overflow: hidden;
}

.card.active{
  transform: scale(1,1);
}

.card-title {
  padding-left: 25rpx;
  margin-top: 25rpx;
  font-size: 0;
  display: flex;
}
.tag{
  width: 88rpx;
  height: 32rpx;
  border-radius: 30rpx;
  border: 1rpx solid #FFFFFF;
  color: #FFFFFF;
  font-size: 20rpx;
  line-height: 30rpx;
  text-align: center;
}

.card-logo {
  margin-right: 12rpx;
  width: 82rpx;
  height: 82rpx;
  border-radius: 50%;
  vertical-align: middle;
}

.card-name {
  font-size: 46rpx;
  margin-bottom: 4rpx;
  width: 480rpx;
}

.card-info {
  position: absolute;
  bottom: 16rpx;
  width: 100%;
  text-align: right;
  padding: 0 20rpx;
  /* display: flex;
  align-items: center; */
}

.card-system {
  width: 72rpx;
  height: 34rpx;
  border-radius: 4rpx;
  background: rgba(0, 0, 0, 0.5);
}

.card-price {
  flex: 1;
  font-size: 48rpx;
}

.buy-line > view{
  display: inline-block;
}

.buy-button{
  height:66rpx;
  line-height:66rpx;
  padding: 0 34rpx;
  background-color:#fff;
  color:#f52d56;
  font-size:30rpx;
  border-radius:36rpx;
}

.buy-icon{
  width:104rpx;
  height:94rpx;
  background-image:url('data:image/png;base64,');
  background-size:100% 100%;
}

.bottom-button.unable{
  background-color: #CCCCCC;
}

.power {
  width: 100%;
  height: auto;
  padding: 0 32rpx;
}

.powerTitle {
  width: 100%;
  line-height: 48rpx;
  color: #484848;
  font-size: 34rpx;
  text-align: left;
  margin-bottom: 28rpx;
  font-weight: 600;
}

.powerBody {
  width: 100%;
  height: auto;
}

.powerItem {
  width: 100%;
  line-height: 48rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
}

.point {
  margin-right: 10rpx;
  background-color: black;
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
}

.powerText{
  position: relative;
  font-size:28rpx;
  padding-left:28rpx;
  color:#666;
}

.powerText::before{
  position: absolute;
  left:0;
  top:0;
  content:'â— ';
  color:#000;
}

.detail-text {
  margin-left: 10rpx;
  color: #58c9f3;
}

.notes-container{
  margin-top:80rpx;
  padding-bottom:200rpx;
}

.notes {
  margin: 0 32rpx;
  border-bottom: 1px solid #efeff4;
}

.notes:first-child{
  border-top:1px solid #efeff4;
}

.notes-title {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
}

.notes-title.border-top {
  border-top: 1px solid #efeff4;
}

.notes-title > view {
  height: 100%;
  display: inline-block;
}

.notes-title-left {
  float: left;
  font-weight: 600;
  color: #484848;
}

.notes-title-right {
  float: right;
  color: #aaa;
}

.notes-title-right > text {
  background: no-repeat right center;
  background-size: 18rpx 12rpx;
  padding-right: 24rpx;
}

.notes-detail{
  font-size:28rpx;
  color:#666;
  padding-bottom:26rpx;
}

.cover{
  position:fixed;
  left:0;
  top:0;
  width:100%;
  height:100%;
  min-height:100vh;
  background: rgba(0, 0, 0, 0.5);
  z-index:1000;
}



