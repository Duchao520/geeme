@import '/utils/wxss/icon.wxss';
page {
  background-color: #fff;
  padding-bottom: 130rpx;
}
.ql-align-center{
  text-align: center;
}


.container {
  width: 100%;
  background-color: #f7f7f7;
}

/* 商品信息 */

.productinfo {
  width: 100%;
  height: auto;
}

.productimg {
  position: relative;
  width: 100%;
  height: 750rpx;
}

.productimg-button {
  position: absolute;
  top: 30rpx;
  width: 76rpx;
  height: 76rpx;
  background-size: 100% 100%;
  z-index: 10;
}

.productimg-button.shopCar{
  width: 76rpx;
  height: 76rpx;
  background: rgba(0,0,0,.3);
  right: 243rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.car-count{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  background: #F52D56;
  top: 0rpx;
  right: 0rpx;
  color: #FFFFFF;
  font-size: 20rpx;
  line-height: 30rpx;
  text-align: center;
}

.productimg-button.collect {
  right: 136rpx;
  background-image: url('data:image/png;base64,');
}

.productimg-button.collect.active {
  background-image: url('data:image/png;base64,');
}

.productimg-button.share {
  right: 32rpx;
  background-image: url(data:image/png;base64,);
}

.swiper-container {
  position: relative;
}

.soldQuantity {
  position: absolute;
  right: 30rpx;
  bottom: 20rpx;
  padding: 0 14rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 20rpx;
  text-align: center;
  background: rgba(0, 0, 0, 0.2);
  color: #fff;
  font-size: 24rpx;
  z-index: 10;
}

/* 拼团价 */
.groupBuy-module{
  position: relative;
  height:120rpx;
}

.groupBuy-left{
  position: absolute;
  width:450rpx;
  height:100%;
  left:0;
  top:0;
  padding-left:30rpx;
  background: #F52D56;
  color:#fff;
}

.groupBuy-left-l1{
  font-size:60rpx;
}

.groupBuy-left-l2{
  margin-top: -10rpx;
  font-size:20rpx;
  line-height:28rpx;
}

.groupBuy-left-l2 > text{
  display: inline-block;
  vertical-align: middle;
  box-sizing: border-box;
  /* width: 56rpx; */
  padding: 0 10rpx;
  height:28rpx;
  line-height:28rpx;
  text-align: center;
  border: 1rpx solid #fff;
  border-radius:14rpx;
}

.groupBuy-right{
  height:100%;
  width: 300rpx;
  position: absolute;
  right: 0;
  line-height:120rpx;
  margin-left:268rpx;
  background: #ffe9e9;
}

.groupBuy-right-headportrait{
  position: relative;
  vertical-align: middle;
  margin-left:-28rpx;
  width: 56rpx;
  height:56rpx;
  border-radius:50%;
  background-image:url('data:image/png;base64,');
  background-size:100% 100%;
  z-index:1;
}

.groupBuy-right-headportrait:first-child{
  margin-left: 20rpx;
  z-index:2;
}
.headPortrait{
  display: block;
  width: 80rpx;
  height: 80rpx;
  margin-right: 18rpx;
  border-radius: 50%;
  background: #9b9b9b;
}

.groupBuy-right-text{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left:8rpx;
  line-height: 50rpx;
  height: 120rpx;
  font-size:30rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
  padding-top: 10rpx;
  color: #333333;
}
.groupBuy-right-time{
  border-radius: 4rpx;
  background-color: rgba(245, 45, 86, 1);
  color: #FFFFFF;
  padding: 0 4rpx;
  margin: 0 6rpx;
}

.groupBuy-right-button{
  display: inline-block;
  vertical-align: middle;
  position: absolute;
  right: 30rpx;
  top:36rpx;
  width:90rpx;
  height:48rpx;
  line-height:48rpx;
  text-align: center;
  color:#fff;
  font-size:28rpx;
  background: #f52d56;
  border-radius:24rpx;
}

/* 生日惊喜价 */
.birthday-module {
  height: 100rpx;
}

.birthday-left {
  float: left;
  width: 522rpx;
  height: 100%;
  line-height: 100rpx;
  background: linear-gradient(to right, #fc3486, #b073fd);
}

.birthday-price {
  display: inline-block;
  height: 100%;
  margin-left: 30rpx;
  color: #fff;
  font-size: 60rpx;
}

.birthday-price-des {
  display: inline-block;
  margin-left:10rpx;
  height:100%;
  vertical-align: top;
  line-height:normal;
}

.birthday-des-triangle {
  display: inline-block;
  position: relative;
  width: 0;
  height: 0;
  border-width: 14rpx 0 0 10rpx;
  border-style: solid;
  border-color: #fff transparent transparent;
}

.birthday-des-text{
  display: inline-block;
  position: relative;
  top:8rpx;
  width: 104rpx;
  height:28rpx;
  line-height:28rpx;
  text-align: center;
  font-size:20rpx;
  color:#fc3587;
  background:#fff;
  border-radius:0 4rpx 4rpx 4rpx;
}

.birthday-right {
  height: 100%;
  background: #ffe1ee;
}

.right-container {
  overflow:hidden;
  margin-left: 522rpx;
  height: 100%;
  text-align: center;
}

.birthday-right-1{
  margin-top:14rpx;
  color:#fc3486;
  font-size:20rpx;
}

.birthday-right-2{
  margin-top:4rpx;
  font-size:0;
}

.birthday-right-2 > view{
  display: inline-block;
  width:34rpx;
  text-align: center;
  border-radius:4rpx;
  background:#fc3486;
  color:#fff;
  font-size:24rpx;
}

.birthday-right-2 > text{
  margin:2rpx;
  font-size:30rpx;
  color:#fc3486;
}

.productdes {
  width: 100%;
  max-height: 128rpx;
  padding: 20rpx 32rpx 0;
  color: #262628;
  font-size: 34rpx;
  line-height: 54rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: box;
  box-orient: vertical;
  line-clamp: 2;
  background-color: white;
}

.productprice {
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
  padding: 0 32rpx;
  background-color: white;
}

.customer-level-module{
  padding:0 32rpx 10rpx;
  background:#fff;
  line-height:normal;
}

.customer-level{
  display: inline-block;
  vertical-align: top;
  padding:0 20rpx;
  height:36rpx;
  line-height:36rpx;
  border-radius:18rpx;
  color:#f52d56;
  font-size:20rpx;
  background:#fde2e7;
}

.price-num {
  display: inline-block;
  height: 100%;
}

.price-num > view {
  display: inline-block;
}

.specialprice {
  color: #f52d56;
  font-size: 40rpx;
  font-weight: 400;
  margin-right: 14rpx;
}

.normalprice {
  color: #9b9b9b;
  font-size: 24rpx;
  text-decoration: line-through;
}

.appointmentbtn {
  float:right;
  margin-top:31rpx;
  width: 120rpx;
  height: 48rpx;
  color: white;
  font-size: 26rpx;
  text-align: center;
  line-height: 48rpx;
  background-color: #f52d56;
  border-radius: 26rpx;
}

/* 优惠信息等 */

.preferential {
  width: 100%;
  height: auto;
  padding: 0 32rpx;
  margin-top: 20rpx;
  background-color: #FFFFFF;
}

.choose {
  width: 100%;
  height: 90rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}

.choose-yes {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
}
.slogan{
  height: 34rpx;
  padding: 4rpx 6rpx;
  border-radius: 4rpx;
  background: #F52D56;
  font-size: 20rpx;
  color: #FFFFFF;
  margin-right: 10rpx;
  margin-top: 6rpx;
}

.plain{
  height: 40rpx;
  line-height: 40rpx;
  border: 2rpx solid #F52D56;
  border-radius: 24rpx;
  font-size: 24rpx;
  color: #F52D56;
  margin: 0;
  padding: 0 20rpx;
}
.hover-plain{
  background: #F52D56;
  color: #FFFFFF;
}

.cy-alt {
  color: #b2b2b2;
  font-size: 28rpx;
  margin-right: 26rpx;
}

.cy-pack {
  color: #333;
  font-size: 28rpx;
}

.packname {
  display: inline-block;
  margin-right: 20rpx;
}

.choose-no {
  line-height: 90rpx;
  color: #666;
  font-size: 28rpx;
}

.moreicontwo {
  width: 11rpx;
  height: 90rpx;
  background: no-repeat center center/100% auto;
  background-size: 12rpx 20rpx;
}

.assessmentHeader {
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 32rpx;
  margin-top: 20rpx;
  background: #fff;
  border-bottom: 1px solid rgba(235, 235, 235, 1);
}

.assessmentHeader-left {
  float: left;
  font-size: 34rpx;
}

.assessmentHeader-left > .amount {
  font-size: 28rpx;
  color: #999;
}

.assessmentHeader-right {
  float: right;
  font-size: 28rpx;
  min-width: 12rpx;
  color: #999;
  padding-right: 20rpx;
  background: no-repeat right center/100% auto;
  background-size: 12rpx 20rpx;
}

/* 底部加入购物车 */

.bottom {
  width: 100%;
  height: 120rpx;
  background-color: white;
  position: fixed;
  bottom: 0;
  left: 0;
  border-top: 1px solid #efeff4;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  z-index: 1;
}

.bottom-left {
  padding: 8rpx 0 0 32rpx;
}

.vip-pricename {
  color: #999;
  font-size: 24rpx;
}

.vip-pricenum {
  color: #f52d56;
  font-size: 48rpx;
  line-height: 72rpx;
}

.bottom-right {
  float: right;
  height: 120rpx;
}

.bottom-right > view {
  display: inline-block;
  width: 196rpx;
  height: 100%;
  color: white;
  font-size: 34rpx;
  text-align: center;
  line-height: 120rpx;
}

.nowbuy {
  background-color: #f52d56;
}

.bottom-2{
  position: fixed;
  bottom:0;
  left:0;
  width:100%;
  height:120rpx;
}

.bottom-2 > view{
  display: inline-block;
  width:50%;
  height:100%;
  font-size:34rpx;
  color:#fff;
  text-align: center;
  padding-top:12rpx;
}

.bottom-2 > view:first-child{
  background: #ff7e99;
}

.bottom-2 > view:last-child{
  background: #f52d56;
}

/* 弹出套餐 */

.package {
  width: 100%;
  height: auto;
  padding: 0 32rpx 170rpx 36rpx;
  position: absolute;
  bottom: -100vh;
  left: 0;
  background-color: white;
  z-index: 999;
  transition: all 0.625s ease;
  border-radius: 6rpx;
}

.package.movepackage {
  bottom: 0;
  z-index: 999;
}

.package.movepackage .totalprice{
  height:120rpx;
  border-top: 2rpx solid #efeff4;
}

.packageavatar {
  width: 154rpx;
  height: 154rpx;
  border-radius: 6rpx;
  border: 2rpx solid white;
  position: absolute;
  left: 36rpx;
  top: -70rpx;
  background-color: black;
}

.packageinfo {
  overflow: hidden;
  margin-top: 24rpx;
  padding-left: 180rpx;
}

.packageinfo-left {
  float: left;
}

.packageinfo-right {
  float: right;
}

.packagename {
  float: left;
  max-height: 108rpx;
  color: #333;
  font-size: 34rpx;
}

.packagestock {
  float: left;
  clear: left;
  margin-top: 8rpx;
  color: #9b9b9b;
  font-size: 24rpx;
}

.packageSpecialPrice {
  color: #f52d56;
  font-size: 34rpx;
  font-weight: 600;
}

.packagePrice {
  float: right;
  text-decoration: line-through;
  color: #9b9b9b;
  font-size: 24rpx;
}

.packagelists {
  width: 100%;
  height: auto;
  padding: 62rpx 0 26rpx;
}

.packagesize {
  width: 100%;
  height: 40rpx;
  text-align: left;
  color: #333;
  font-size: 28rpx;
  margin-bottom: 18rpx;
}

.packageitems {
  width: 100%;
  height: auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
}

.pi-item {
  position: relative;
  margin-right: 30rpx;
  margin-bottom: 30rpx;
  padding: 17rpx;
  font-size: 24rpx;
  color: #989898;
  border: 1rpx solid #979797;
  border-radius: 4rpx;
}

.pi-item.active {
  color: #f52d56;
  border: 1rpx solid #f52d56;
}

.pi-item-symbol{
  position: absolute;
  width:34rpx;
  height:34rpx;
  line-height:34rpx;
  top:-17rpx;
  right:-17rpx;
  background: #f52d56;
  color:#fff;
  font-size:20rpx;
  text-align: center;
  border-radius:50%;
}

.packagecount {
  height: 56rpx;
  line-height: 56rpx;
}

.packagetext {
  float: left;
  color: #333;
  font-size: 28rpx;
}

/* 合计金额 */

.totalprice {
  width: 100%;
