.AppNavBar {
  position: fixed;
  width:100%;
  top:0;
  background-color: #fff;
  overflow:hidden;
}

.navigate-bar {
  position: relative;
  text-align: center;
}

.navigate-icon{
  position: absolute;
  left: 0;
  display: flex;
  align-items: center;
}

.navigator-back{
  width: 64rpx;
  height: 100%;
  background: url('data:image/png;base64,') no-repeat 32rpx center;
}

.navigate-title {
  font-size: 34rpx;
  font-weight: bold;
  color:#3c3c3c;
}
