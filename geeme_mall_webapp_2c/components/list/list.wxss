/* components/list/list.wxss */
@import '../../utils/wxss/base.wxss';

.recommended-item {
  width: 686rpx;
  border-radius: 16rpx;
  background-color: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 0rpx 12rpx 0rpx rgba(0, 0, 0, 0.1);
  padding: 24rpx 20rpx 24rpx 24rpx;

  margin-bottom: 28rpx;
  position: relative;
}
.r-nav{
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}

.recommended-left {
  width: 176rpx;
  height: 176rpx;
  border-radius: 8rpx;
  flex-shrink: 0;
  /* background-color: aqua; */
  /* background: url() no-repeat center center/100% auto; */
}

.recommended-right {
  width: 486rpx;
  padding-left: 20rpx;
}

.recommended-one {
  width: 100%;
  height: 42rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  padding-right: 20rpx;
}

.r-name {
  width: 340rpx;
  height: 100%;
  font-size: 32rpx;
  color: #263238;
  overflow: hidden;
  text-overflow: ellipsis;
  display: box;
  box-orient: vertical;
  white-space: nowrap;
}

.r-distance {
  /* width: 61rpx; */
  height: 32rpx;
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
  line-height: 32rpx;
  display: flex;
  flex-wrap: nowrap;
}


.r-address {
 width: 20rpx;
  height: 32rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}

.recommended-two{
  margin-top:12rpx;
  font-size:0;
}

.recommended-two > text{
  display: inline-block;
  vertical-align: middle;
  color:#f52d56;
  font-size:24rpx;
}

.recommended-three {
  margin-top:12rpx;
  width: 100%;
  height: 32rpx;
  font-size: 24rpx;
  color: rgba(153, 153, 153, .7);
  text-align: left;
  line-height: 32rpx;
}

.recommended-four {
  margin-top:12rpx;
  width: 93%;
  font-size: 24rpx;
  color: rgba(153, 153, 153,1);
  text-align: left;
  line-height: 32rpx;                                                                                         
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.phone{
  width: 100rpx;
  height: 100rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  background-position: 48rpx 9rpx;
  background-size: 32rpx 32rpx;
  position: absolute;
  top: 110rpx;
  right: 20rpx;
}

.r-name > text{
  display: inline-block;
  vertical-align: top;
}

.city{
  max-width:100rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.model{
  width: 134rpx;
}
.model image{
  display: block;
  width: 134rpx;
  height: 134rpx;
  background: #cccccc;
}